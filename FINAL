    4: {"–ê–ò-92": "–ê–ò-92 ‚Ññ1", "–ê–ò-95": "–ê–ò-95 ‚Ññ1", "–ê–ò-98": "–ê–ò-98 ‚Ññ1", "–î–¢": "–î–¢ ‚Ññ1"},
    5: {"–ê–ò-92": "–ê–ò-92 ‚Ññ1", "–ê–ò-95": "–ê–ò-95 ‚Ññ2", "–ê–ò-98": "–ê–ò-98 ‚Ññ1", "–î–¢": "–î–¢ ‚Ññ1"},
    6: {"–ê–ò-92": "–ê–ò-92 ‚Ññ1", "–ê–ò-95": "–ê–ò-95 ‚Ññ2", "–ê–ò-98": "–ê–ò-98 ‚Ññ1", "–î–¢": "–î–¢ ‚Ññ1"},
    7: {"–ê–ò-95": "–ê–ò-95 ‚Ññ2", "–î–¢": "–î–¢ ‚Ññ1"},
