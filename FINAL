        input("\n–ù–∞–∂–º–∏—Ç–µ Enter...")
    
    def emergency_mode(self):
        """–†–µ–∂–∏–º –∞–≤–∞—Ä–∏–∏"""
        print("\n" + "="*40)
        print("–ê–í–ê–†–ò–ô–ù–´–ô –†–ï–ñ–ò–ú")
        print("="*40)
        
        if self.emergency["active"]:
            print(f"\n‚ö†Ô∏è  –ê–≤–∞—Ä–∏–π–Ω—ã–π —Ä–µ–∂–∏–º –∞–∫—Ç–∏–≤–µ–Ω —Å {self.emergency['time']}")
            print("\n1) –û—Ç–∫–ª—é—á–∏—Ç—å –∞–≤–∞—Ä–∏–π–Ω—ã–π —Ä–µ–∂–∏–º")
            print("2) –í–µ—Ä–Ω—É—Ç—å—Å—è")
            
            try:
                choice = int(input("\n–í—ã–±–æ—Ä: "))
            except:
                print("–û—à–∏–±–∫–∞ –≤–≤–æ–¥–∞")
                return
            
            if choice == 1:
                confirm = input("\n–í—ã —É–≤–µ—Ä–µ–Ω—ã? (y/n): ").lower()
                if confirm == 'y':
                    self.emergency["active"] = False
                    self.save_all()
                    print("\n‚úÖ –ê–≤–∞—Ä–∏–π–Ω—ã–π —Ä–µ–∂–∏–º –æ—Ç–∫–ª—é—á–µ–Ω")
                    print("–ü—Ä–∏–º–µ—á–∞–Ω–∏–µ: —Ü–∏—Å—Ç–µ—Ä–Ω—ã –æ—Å—Ç–∞–ª–∏—Å—å –≤—ã–∫–ª—é—á–µ–Ω–Ω—ã–º–∏")
                else:
                    print("–û—Ç–º–µ–Ω–µ–Ω–æ")
        else:
            print("\nüö® –í–ù–ò–ú–ê–ù–ò–ï! –ê–∫—Ç–∏–≤–∞—Ü–∏—è –∞–≤–∞—Ä–∏–π–Ω–æ–≥–æ —Ä–µ–∂–∏–º–∞!")
            print("\n–ü–æ—Å–ª–µ–¥—Å—Ç–≤–∏—è:")
            print("‚Ä¢ –í—Å–µ —Ü–∏—Å—Ç–µ—Ä–Ω—ã –±—É–¥—É—Ç –æ—Ç–∫–ª—é—á–µ–Ω—ã")
            print("‚Ä¢ –ó–∞–ø—Ä–∞–≤–∫–∞ –æ—Å—Ç–∞–Ω–æ–≤–∏—Ç—Å—è")
            print("‚Ä¢ –í—ã–∑–æ–≤ –∞–≤–∞—Ä–∏–π–Ω—ã—Ö —Å–ª—É–∂–±")
            
            confirm = input("\n–ü–æ–¥—Ç–≤–µ—Ä–¥–∏—Ç—å –∞–∫—Ç–∏–≤–∞—Ü–∏—é? (y/n): ").lower()
            if confirm == 'y':
                # –û—Ç–∫–ª—é—á–∞–µ–º –≤—Å–µ —Ü–∏—Å—Ç–µ—Ä–Ω—ã
                for tank in self.tanks.values():
                    tank["enabled"] = False
                
                self.emergency["active"] = True
                self.emergency["time"] = datetime.now().strftime("%Y-%m-%d %H:%M:%S")
                
                self.add_history("–ê–í–ê–†–ò–Ø", "–ê–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω –∞–≤–∞—Ä–∏–π–Ω—ã–π —Ä–µ–∂–∏–º")
                self.save_all()
                
                print("\nüö® –ê–í–ê–†–ò–ô–ù–´–ô –†–ï–ñ–ò–ú –ê–ö–¢–ò–í–ò–†–û–í–ê–ù!")
                print("–í—Å–µ —Ü–∏—Å—Ç–µ—Ä–Ω—ã –æ—Ç–∫–ª—é—á–µ–Ω—ã")
                print("–í—ã–∑–æ–≤ —Å–ª—É–∂–±: 112, 104")
            else:
                print("–û—Ç–º–µ–Ω–µ–Ω–æ")
        
        input("\n–ù–∞–∂–º–∏—Ç–µ Enter...")
    
    def main_menu(self):
        """–ì–ª–∞–≤–Ω–æ–µ –º–µ–Ω—é"""
        while True:
            os.system('cls' if os.name == 'nt' else 'clear')
            
            # –ó–∞–≥–æ–ª–æ–≤–æ–∫
            print("="*50)
            print("–ê–ó–° '–ù–ï–§–¢–ï–°–û–§–¢'")
            print("–°–∏—Å—Ç–µ–º–∞ —É–ø—Ä–∞–≤–ª–µ–Ω–∏—è")
            print("="*50)
            
            # –ü—Ä–µ–¥—É–ø—Ä–µ–∂–¥–µ–Ω–∏—è
            if self.emergency["active"]:
                print("\nüö®üö®üö® –ê–í–ê–†–ò–ô–ù–´–ô –†–ï–ñ–ò–ú üö®üö®üö®")
                print("–ó–∞–ø—Ä–∞–≤–∫–∞ –Ω–µ —Ä–∞–±–æ—Ç–∞–µ—Ç!")
            else:
                disabled = self.get_disabled_tanks()
                if disabled:
                    print("\n–í–ù–ò–ú–ê–ù–ò–ï! –û—Ç–∫–ª—é—á–µ–Ω–Ω—ã–µ —Ü–∏—Å—Ç–µ—Ä–Ω—ã:")
                    for tank in disabled:
                        print(f"  ‚Ä¢ {tank}")
            
            # –ú–µ–Ω—é
            print("\n" + "-"*50)
            print("–ì–õ–ê–í–ù–û–ï –ú–ï–ù–Æ")
            print("-"*50)
            print("\n1) –û–±—Å–ª—É–∂–∏—Ç—å –∫–ª–∏–µ–Ω—Ç–∞")
            print("2) –°–æ—Å—Ç–æ—è–Ω–∏–µ —Ü–∏—Å—Ç–µ—Ä–Ω")
            print("3) –ü–æ–ø–æ–ª–Ω–∏—Ç—å —Ü–∏—Å—Ç–µ—Ä–Ω—É")
            print("4) –°—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞")
            print("5) –ò—Å—Ç–æ—Ä–∏—è –æ–ø–µ—Ä–∞—Ü–∏–π")
            print("6) –ü–µ—Ä–µ–∫–∞—á–∫–∞ —Ç–æ–ø–ª–∏–≤–∞")
            print("7) –£–ø—Ä–∞–≤–ª–µ–Ω–∏–µ —Ü–∏—Å—Ç–µ—Ä–Ω–∞–º–∏")
            print("8) –°–æ—Å—Ç–æ—è–Ω–∏–µ –∫–æ–ª–æ–Ω–æ–∫")
            print("9) –ê–í–ê–†–ò–ô–ù–´–ô –†–ï–ñ–ò–ú")
            print("0) –í—ã—Ö–æ–¥")
            
            try:
                choice = int(input("\n> "))
            except:
                continue
            
            if choice == 0:
                print("\n–°–æ—Ö—Ä–∞–Ω–µ–Ω–∏–µ –¥–∞–Ω–Ω—ã—Ö...")
                self.save_all()
                print("–í—ã—Ö–æ–¥")
                break
            
            elif choice == 1:
                self.serve_customer()
            elif choice == 2:
                self.show_tanks()
            elif choice == 3:
                self.refuel_tank()
            elif choice == 4:
                self.show_stats()
            elif choice == 5:
                self.show_history()
            elif choice == 6:
                self.transfer_fuel()
            elif choice == 7:
                self.manage_tanks()
            elif choice == 8:
                self.show_columns()
            elif choice == 9:
                self.emergency_mode()
            else:
                print("–ù–µ–≤–µ—Ä–Ω—ã–π –≤—ã–±–æ—Ä")
                input("\n–ù–∞–∂–º–∏—Ç–µ Enter...")


def main():
    """–ó–∞–ø—É—Å–∫ –ø—Ä–æ–≥—Ä–∞–º–º—ã"""
    print("–ó–∞–≥—Ä—É–∑–∫–∞ —Å–∏—Å—Ç–µ–º—ã –ê–ó–°...")
    
    # –°–æ–∑–¥–∞–µ–º –ø–∞–ø–∫—É –¥–ª—è –¥–∞–Ω–Ω—ã—Ö
    os.makedirs("data", exist_ok=True)
    
    # –ó–∞–ø—É—Å–∫–∞–µ–º —Å–∏—Å—Ç–µ–º—É
    azs = AZS()
    azs.main_menu()


if __name__ == "__main__":
    main()
        
       
