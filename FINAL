    def –≤_—Å–ª–æ–≤–∞—Ä—å(self):
        """–ü—Ä–µ–æ–±—Ä–∞–∑—É–µ—Ç –≤ —Å–ª–æ–≤–∞—Ä—å –¥–ª—è —Å–æ—Ö—Ä–∞–Ω–µ–Ω–∏—è"""
        return {
            "–∏–¥": self.–∏–¥,
            "—Ç–∏–ø": self.—Ç–∏–ø,
            "–º–∞–∫—Å": self.–º–∞–∫—Å,
            "—Ç–µ–∫—É—â–∏–π": self.—Ç–µ–∫—É—â–∏–π,
            "–≤–∫–ª—é—á–µ–Ω–∞": self.–≤–∫–ª—é—á–µ–Ω–∞,
            "–∞–≤—Ç–æ–æ—Ç–∫–ª": self.–∞–≤—Ç–æ–æ—Ç–∫–ª
        }
    
    @staticmethod
    def –∏–∑_—Å–ª–æ–≤–∞—Ä—è(–¥–∞–Ω–Ω—ã–µ):
        """–°–æ–∑–¥–∞–µ—Ç –æ–±—ä–µ–∫—Ç –∏–∑ —Å–ª–æ–≤–∞—Ä—è"""
        —Ü–∏—Å—Ç–µ—Ä–Ω–∞ = –¶–∏—Å—Ç–µ—Ä–Ω–∞(
            –∏–¥–µ–Ω—Ç–∏—Ñ–∏–∫–∞—Ç–æ—Ä=–¥–∞–Ω–Ω—ã–µ["–∏–¥"],
            —Ç–∏–ø_—Ç–æ–ø–ª–∏–≤–∞=–¥–∞–Ω–Ω—ã–µ["—Ç–∏–ø"],
            –æ–±—ä–µ–º_–º–∞–∫—Å=–¥–∞–Ω–Ω—ã–µ["–º–∞–∫—Å"],
            –æ–±—ä–µ–º_—Ç–µ–∫—É—â–∏–π=–¥–∞–Ω–Ω—ã–µ["—Ç–µ–∫—É—â–∏–π"]
        )
        —Ü–∏—Å—Ç–µ—Ä–Ω–∞.–≤–∫–ª—é—á–µ–Ω–∞ = –¥–∞–Ω–Ω—ã–µ["–≤–∫–ª—é—á–µ–Ω–∞"]
        —Ü–∏—Å—Ç–µ—Ä–Ω–∞.–∞–≤—Ç–æ–æ—Ç–∫–ª = –¥–∞–Ω–Ω—ã–µ.get("–∞–≤—Ç–æ–æ—Ç–∫–ª", False)
        return —Ü–∏—Å—Ç–µ—Ä–Ω–∞

# ============================================
# –û–°–ù–û–í–ù–û–ô –ö–õ–ê–°–° –°–ò–°–¢–ï–ú–´ –ê–ó–°
# ============================================

class –°–∏—Å—Ç–µ–º–∞–ê–ó–°:
    """–ì–ª–∞–≤–Ω—ã–π –∫–ª–∞—Å—Å —Å–∏—Å—Ç–µ–º—ã —É–ø—Ä–∞–≤–ª–µ–Ω–∏—è –ê–ó–°"""
    
    def __init__(self):
        # –ó–∞–≥—Ä—É–∂–∞–µ–º –≤—Å–µ –¥–∞–Ω–Ω—ã–µ
        self.–∑–∞–≥—Ä—É–∑–∏—Ç—å_–≤—Å–µ_–¥–∞–Ω–Ω—ã–µ()
        
        # –°—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞ (–µ—Å–ª–∏ –Ω–µ –∑–∞–≥—Ä—É–∑–∏–ª–∞—Å—å)
        if not self.—Å—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞:
            self.—Å—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞ = {
                "–≤—Å–µ–≥–æ_–º–∞—à–∏–Ω": 0,
                "–æ–±—â–∏–π_–¥–æ—Ö–æ–¥": 0.0,
                "–ø–æ_—Ç–∏–ø–∞–º": {}
            }
            for —Ç–∏–ø in –¶–ï–ù–´_–¢–û–ü–õ–ò–í–ê:
                self.—Å—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞["–ø–æ_—Ç–∏–ø–∞–º"][—Ç–∏–ø] = {
                    "–ª–∏—Ç—Ä–æ–≤": 0.0,
                    "–¥–æ—Ö–æ–¥": 0.0,
                    "—Ç—Ä–∞–Ω–∑–∞–∫—Ü–∏–π": 0
                }
        
        # –ò—Å—Ç–æ—Ä–∏—è (–µ—Å–ª–∏ –Ω–µ –∑–∞–≥—Ä—É–∑–∏–ª–∞—Å—å)
        if not self.–∏—Å—Ç–æ—Ä–∏—è:
            self.–∏—Å—Ç–æ—Ä–∏—è = []
        
        # –ê–≤–∞—Ä–∏—è (–µ—Å–ª–∏ –Ω–µ –∑–∞–≥—Ä—É–∑–∏–ª–∞—Å—å)
        if not self.–∞–≤–∞—Ä–∏—è:
            self.–∞–≤–∞—Ä–∏—è = {"–∞–∫—Ç–∏–≤–Ω–∞": False, "–≤—Ä–µ–º—è": None}
    
    def –∑–∞–≥—Ä—É–∑–∏—Ç—å_–≤—Å–µ_–¥–∞–Ω–Ω—ã–µ(self):
        """–ó–∞–≥—Ä—É–∂–∞–µ—Ç –≤—Å–µ –¥–∞–Ω–Ω—ã–µ –∏–∑ —Ñ–∞–π–ª–æ–≤"""
        # –ó–∞–≥—Ä—É–∂–∞–µ–º —Ü–∏—Å—Ç–µ—Ä–Ω—ã
        –¥–∞–Ω–Ω—ã–µ = –∑–∞–≥—Ä—É–∑–∏—Ç—å_–¥–∞–Ω–Ω—ã–µ(–§–ê–ô–õ_–¶–ò–°–¢–ï–†–ù–´)
        if –¥–∞–Ω–Ω—ã–µ:
            self.—Ü–∏—Å—Ç–µ—Ä–Ω—ã = {}
            for –∏–¥, –∏–Ω—Ñ–æ in –¥–∞–Ω–Ω—ã—Ö.items():
                self.—Ü–∏—Å—Ç–µ—Ä–Ω—ã[–∏–¥] = –¶–∏—Å—Ç–µ—Ä–Ω–∞.–∏–∑_—Å–ª–æ–≤–∞—Ä—è(–∏–Ω—Ñ–æ)
        else:
            # –°–æ–∑–¥–∞–µ–º —Ü–∏—Å—Ç–µ—Ä–Ω—ã –ø–æ —É–º–æ–ª—á–∞–Ω–∏—é
            self.—Ü–∏—Å—Ç–µ—Ä–Ω—ã = self.—Å–æ–∑–¥–∞—Ç—å_—Ü–∏—Å—Ç–µ—Ä–Ω—ã_–ø–æ_—É–º–æ–ª—á–∞–Ω–∏—é()
        
        # –ó–∞–≥—Ä—É–∂–∞–µ–º —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫—É
        self.—Å—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞ = –∑–∞–≥—Ä—É–∑–∏—Ç—å_–¥–∞–Ω–Ω—ã–µ(–§–ê–ô–õ_–°–¢–ê–¢–ò–°–¢–ò–ö–ê)
        
        # –ó–∞–≥—Ä—É–∂–∞–µ–º –∏—Å—Ç–æ—Ä–∏—é
        self.–∏—Å—Ç–æ—Ä–∏—è = –∑–∞–≥—Ä—É–∑–∏—Ç—å_–¥–∞–Ω–Ω—ã–µ(–§–ê–ô–õ_–ò–°–¢–û–†–ò–Ø, [])
        
        # –ó–∞–≥—Ä—É–∂–∞–µ–º –∞–≤–∞—Ä–∏—é
        self.–∞–≤–∞—Ä–∏—è = –∑–∞–≥—Ä—É–∑–∏—Ç—å_–¥–∞–Ω–Ω—ã–µ(–§–ê–ô–õ_–ê–í–ê–†–ò–Ø, {})
    
    def —Å–æ—Ö—Ä–∞–Ω–∏—Ç—å_–≤—Å–µ_–¥–∞–Ω–Ω—ã–µ(self):
        """–°–æ—Ö—Ä–∞–Ω—è–µ—Ç –≤—Å–µ –¥–∞–Ω–Ω—ã–µ –≤ —Ñ–∞–π–ª—ã"""
        # –°–æ—Ö—Ä–∞–Ω—è–µ–º —Ü–∏—Å—Ç–µ—Ä–Ω—ã
        –¥–∞–Ω–Ω—ã–µ = {}
        for –∏–¥, —Ü–∏—Å—Ç–µ—Ä–Ω–∞ in self.—Ü–∏—Å—Ç–µ—Ä–Ω—ã.items():
            –¥–∞–Ω–Ω—ã–µ[–∏–¥] = —Ü–∏—Å—Ç–µ—Ä–Ω–∞.–≤_—Å–ª–æ–≤–∞—Ä—å()
        —Å–æ—Ö—Ä–∞–Ω–∏—Ç—å_–¥–∞–Ω–Ω—ã–µ(–§–ê–ô–õ_–¶–ò–°–¢–ï–†–ù–´, –¥–∞–Ω–Ω—ã–µ)
        
        # –°–æ—Ö—Ä–∞–Ω—è–µ–º —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫—É
        —Å–æ—Ö—Ä–∞–Ω–∏—Ç—å_–¥–∞–Ω–Ω—ã–µ(–§–ê–ô–õ_–°–¢–ê–¢–ò–°–¢–ò–ö–ê, self.—Å—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞)
        
        # –°–æ—Ö—Ä–∞–Ω—è–µ–º –∏—Å—Ç–æ—Ä–∏—é
        —Å–æ—Ö—Ä–∞–Ω–∏—Ç—å_–¥–∞–Ω–Ω—ã–µ(–§–ê–ô–õ_–ò–°–¢–û–†–ò–Ø, self.–∏—Å—Ç–æ—Ä–∏—è)
        
        # –°–æ—Ö—Ä–∞–Ω—è–µ–º –∞–≤–∞—Ä–∏—é
        —Å–æ—Ö—Ä–∞–Ω–∏—Ç—å_–¥–∞–Ω–Ω—ã–µ(–§–ê–ô–õ_–ê–í–ê–†–ò–Ø, self.–∞–≤–∞—Ä–∏—è)
    
    def —Å–æ–∑–¥–∞—Ç—å_—Ü–∏—Å—Ç–µ—Ä–Ω—ã_–ø–æ_—É–º–æ–ª—á–∞–Ω–∏—é(self):
        """–°–æ–∑–¥–∞–µ—Ç —Ü–∏—Å—Ç–µ—Ä–Ω—ã –ø–æ –¢–ó"""
        —Ü–∏—Å—Ç–µ—Ä–Ω—ã = {}
        
        # –°–æ–∑–¥–∞–µ–º –≤—Å–µ —Ü–∏—Å—Ç–µ—Ä–Ω—ã —Å–æ–≥–ª–∞—Å–Ω–æ –¢–ó
        —Ü–∏—Å—Ç–µ—Ä–Ω—ã["–ê–ò-92 ‚Ññ1"] = –¶–∏—Å—Ç–µ—Ä–Ω–∞("–ê–ò-92 ‚Ññ1", "–ê–ò-92", 20000, 12400)
        —Ü–∏—Å—Ç–µ—Ä–Ω—ã["–ê–ò-95 ‚Ññ1"] = –¶–∏—Å—Ç–µ—Ä–Ω–∞("–ê–ò-95 ‚Ññ1", "–ê–ò-95", 20000, 9800)
        —Ü–∏—Å—Ç–µ—Ä–Ω—ã["–ê–ò-95 ‚Ññ2"] = –¶–∏—Å—Ç–µ—Ä–Ω–∞("–ê–ò-95 ‚Ññ2", "–ê–ò-95", 20000, 1200)
        —Ü–∏—Å—Ç–µ—Ä–Ω—ã["–ê–ò-98 ‚Ññ1"] = –¶–∏—Å—Ç–µ—Ä–Ω–∞("–ê–ò-98 ‚Ññ1", "–ê–ò-98", 15000, 10000)
        —Ü–∏—Å—Ç–µ—Ä–Ω—ã["–î–¢ ‚Ññ1"] = –¶–∏—Å—Ç–µ—Ä–Ω–∞("–î–¢ ‚Ññ1", "–î–¢", 25000, 15600)
        
        # –û—Ç–∫–ª—é—á–∞–µ–º —Ç–µ, —á—Ç–æ –Ω–∏–∂–µ –º–∏–Ω–∏–º—É–º–∞
        —Ü–∏—Å—Ç–µ—Ä–Ω—ã["–ê–ò-95 ‚Ññ2"].–≤—ã–∫–ª—é—á–∏—Ç—å()
        —Ü–∏—Å—Ç–µ—Ä–Ω—ã["–ê–ò-95 ‚Ññ2"].–∞–≤—Ç–æ–æ—Ç–∫–ª = True
        —Ü–∏—Å—Ç–µ—Ä–Ω—ã["–ê–ò-98 ‚Ññ1"].–≤—ã–∫–ª—é—á–∏—Ç—å()
        
        return —Ü–∏—Å—Ç–µ—Ä–Ω—ã
    
    def –¥–æ–±–∞–≤–∏—Ç—å_–≤_–∏—Å—Ç–æ—Ä–∏—é(self, —Ç–∏–ø, –æ–ø–∏—Å–∞–Ω–∏–µ, –¥–µ—Ç–∞–ª–∏=None):
        """–î–æ–±–∞–≤–ª—è–µ—Ç –∑–∞–ø–∏—Å—å –≤ –∏—Å—Ç–æ—Ä–∏—é –æ–ø–µ—Ä–∞—Ü–∏–π"""
        –∑–∞–ø–∏—Å—å = {
            "–≤—Ä–µ–º—è": –ø–æ–ª—É—á–∏—Ç—å_–¥–∞—Ç—É_–≤—Ä–µ–º—è(),
            "—Ç–∏–ø": —Ç–∏–ø,
            "–æ–ø–∏—Å–∞–Ω–∏–µ": –æ–ø–∏—Å–∞–Ω–∏–µ,
            "–¥–µ—Ç–∞–ª–∏": –¥–µ—Ç–∞–ª–∏ or {}
        }
        self.–∏—Å—Ç–æ—Ä–∏—è.append(–∑–∞–ø–∏—Å—å)
        
        # –û–≥—Ä–∞–Ω–∏—á–∏–≤–∞–µ–º –∏—Å—Ç–æ—Ä–∏—é 100 –∑–∞–ø–∏—Å—è–º–∏
        if len(self.–∏—Å—Ç–æ—Ä–∏—è) > 100:
            self.–∏—Å—Ç–æ—Ä–∏—è = self.–∏—Å—Ç–æ—Ä–∏—è[-100:]
    
    def –ø—Ä–æ–≤–µ—Ä–∏—Ç—å_–æ—Ç–∫–ª—é—á–µ–Ω–Ω—ã–µ_—Ü–∏—Å—Ç–µ—Ä–Ω—ã(self):
        """–í–æ–∑–≤—Ä–∞—â–∞–µ—Ç —Å–ø–∏—Å–æ–∫ –æ—Ç–∫–ª—é—á–µ–Ω–Ω—ã—Ö —Ü–∏—Å—Ç–µ—Ä–Ω"""
        –æ—Ç–∫–ª—é—á–µ–Ω—ã = []
        for —Ü–∏—Å—Ç–µ—Ä–Ω–∞ in self.—Ü–∏—Å—Ç–µ—Ä–Ω—ã.values():
            if –Ω–µ —Ü–∏—Å—Ç–µ—Ä–Ω–∞.–≤–∫–ª—é—á–µ–Ω–∞:
                –ø—Ä–∏—á–∏–Ω–∞ = "–Ω–∏–∑–∫–∏–π —É—Ä–æ–≤–µ–Ω—å" if —Ü–∏—Å—Ç–µ—Ä–Ω–∞.–∞–≤—Ç–æ–æ—Ç–∫–ª else "—Ä—É—á–Ω–æ–µ –æ—Ç–∫–ª—é—á–µ–Ω–∏–µ"
                –æ—Ç–∫–ª—é—á–µ–Ω—ã.append(f"{—Ü–∏—Å—Ç–µ—Ä–Ω–∞.–∏–¥} ({–ø—Ä–∏—á–∏–Ω–∞})")
        return –æ—Ç–∫–ª—é—á–µ–Ω—ã
    
    # ============================================
    # –û–°–ù–û–í–ù–´–ï –§–£–ù–ö–¶–ò–ò –°–ò–°–¢–ï–ú–´
    # ============================================
    
    def –æ–±—Å–ª—É–∂–∏—Ç—å_–∫–ª–∏–µ–Ω—Ç–∞(self):
        """–§—É–Ω–∫—Ü–∏—è –æ–±—Å–ª—É–∂–∏–≤–∞–Ω–∏—è –∫–ª–∏–µ–Ω—Ç–∞ (–∫–∞—Å—Å–∞)"""
        if self.–∞–≤–∞—Ä–∏—è["–∞–∫—Ç–∏–≤–Ω–∞"]:
            print("\n–û–®–ò–ë–ö–ê: –ê–∫—Ç–∏–≤–µ–Ω –∞–≤–∞—Ä–∏–π–Ω—ã–π —Ä–µ–∂–∏–º!")
            print("–û–±—Å–ª—É–∂–∏–≤–∞–Ω–∏–µ –∫–ª–∏–µ–Ω—Ç–æ–≤ –Ω–µ–≤–æ–∑–º–æ–∂–Ω–æ.")
            input("\n–ù–∞–∂–º–∏—Ç–µ Enter...")
            return
        
        print("\n" + "="*50)
        print("–û–ë–°–õ–£–ñ–ò–í–ê–ù–ò–ï –ö–õ–ò–ï–ù–¢–ê")
        print("="*50)
        
        # 1. –í—ã–±–æ—Ä –∫–æ–ª–æ–Ω–∫–∏
        print("\n–í—ã–±–µ—Ä–∏—Ç–µ –∫–æ–ª–æ–Ω–∫—É (1-8):")
        for i in range(1, 9):
            print(f"  {i}) –ö–æ–ª–æ–Ω–∫–∞ {i}")
        
        –∫–æ–ª–æ–Ω–∫–∞, –æ—à–∏–±–∫–∞ = –ø—Ä–æ–≤–µ—Ä–∏—Ç—å_–≤–≤–æ–¥_—Ü–µ–ª–æ–≥–æ(input("\n> "), 1, 8)
        if –æ—à–∏–±–∫–∞:
            print(f"–û—à–∏–±–∫–∞: {–æ—à–∏–±–∫–∞}")
            input("\n–ù–∞–∂–º–∏—Ç–µ Enter...")
            return
        
        # –ü—Ä–æ–≤–µ—Ä—è–µ–º, –∫–∞–∫–∏–µ —Ç–∏–ø—ã —Ç–æ–ø–ª–∏–≤–∞ –¥–æ—Å—Ç—É–ø–Ω—ã –Ω–∞ —ç—Ç–æ–π –∫–æ–ª–æ–Ω–∫–µ
        if –∫–æ–ª–æ–Ω–∫–∞ not in –°–•–ï–ú–ê_–ö–û–õ–û–ù–û–ö:
            print("–û—à–∏–±–∫–∞: —Ç–∞–∫–∞—è –∫–æ–ª–æ–Ω–∫–∞ –Ω–µ —Å—É—â–µ—Å—Ç–≤—É–µ—Ç")
            input("\n–ù–∞–∂–º–∏—Ç–µ Enter...")
            return
        
        –¥–æ—Å—Ç—É–ø–Ω—ã–µ_—Ç–∏–ø—ã = –°–•–ï–ú–ê_–ö–û–õ–û–ù–û–ö[–∫–æ–ª–æ–Ω–∫–∞]
        
        print(f"\n–ö–æ–ª–æ–Ω–∫–∞ {–∫–æ–ª–æ–Ω–∫–∞} - –¥–æ—Å—Ç—É–ø–Ω—ã–µ –≤–∏–¥—ã —Ç–æ–ø–ª–∏–≤–∞:")
        –≤–∞—Ä–∏–∞–Ω—Ç—ã = []
        –Ω–æ–º–µ—Ä = 1
        
        for —Ç–∏–ø_—Ç–æ–ø–ª–∏–≤–∞, id_—Ü–∏—Å—Ç–µ—Ä–Ω—ã in –¥–æ—Å—Ç—É–ø–Ω—ã–µ_—Ç–∏–ø—ã.items():
            if id_—Ü–∏—Å—Ç–µ—Ä–Ω—ã in self.—Ü–∏—Å—Ç–µ—Ä–Ω—ã:
                —Ü–∏—Å—Ç–µ—Ä–Ω–∞ = self.—Ü–∏—Å—Ç–µ—Ä–Ω—ã[id_—Ü–∏—Å—Ç–µ—Ä–Ω—ã]
                —Å—Ç–∞—Ç—É—Å = "‚úì" if —Ü–∏—Å—Ç–µ—Ä–Ω–∞.–≤–∫–ª—é—á–µ–Ω–∞ else "‚úó"
                print(f"  {–Ω–æ–º–µ—Ä}) {—Ç–∏–ø_—Ç–æ–ø–ª–∏–≤–∞} (—Ü–∏—Å—Ç–µ—Ä–Ω–∞ {id_—Ü–∏—Å—Ç–µ—Ä–Ω—ã}) [{—Å—Ç–∞—Ç—É—Å}]")
                –≤–∞—Ä–∏–∞–Ω—Ç—ã.append((—Ç–∏–ø_—Ç–æ–ø–ª–∏–≤–∞, id_—Ü–∏—Å—Ç–µ—Ä–Ω—ã))
                –Ω–æ–º–µ—Ä += 1
        
        if –Ω–µ –≤–∞—Ä–∏–∞–Ω—Ç—ã:
            print("  –ù–µ—Ç –¥–æ—Å—Ç—É–ø–Ω–æ–≥–æ —Ç–æ–ø–ª–∏–≤–∞")
            input("\n–ù–∞–∂–º–∏—Ç–µ Enter...")
            return
        
        # 2. –í—ã–±–æ—Ä —Ç–æ–ø–ª–∏–≤–∞
        –≤—ã–±–æ—Ä, –æ—à–∏–±–∫–∞ = –ø—Ä–æ–≤–µ—Ä–∏—Ç—å_–≤–≤–æ–¥_—Ü–µ–ª–æ–≥–æ(input("\n–í—ã–±–µ—Ä–∏—Ç–µ —Ç–æ–ø–ª–∏–≤–æ: "), 1, len(–≤–∞—Ä–∏–∞–Ω—Ç—ã))
        if –æ—à–∏–±–∫–∞:
            print(f"–û—à–∏–±–∫–∞: {–æ—à–∏–±–∫–∞}")
            input("\n–ù–∞–∂–º–∏—Ç–µ Enter...")
            return
        
        —Ç–∏–ø_—Ç–æ–ø–ª–∏–≤–∞, id_—Ü–∏—Å—Ç–µ—Ä–Ω—ã = –≤–∞—Ä–∏–∞–Ω—Ç—ã[–≤—ã–±–æ—Ä - 1]
        —Ü–∏—Å—Ç–µ—Ä–Ω–∞ = self.—Ü–∏—Å—Ç–µ—Ä–Ω—ã[id_—Ü–∏—Å—Ç–µ—Ä–Ω—ã]
        
        # –ü—Ä–æ–≤–µ—Ä–∫–∞ —Å–æ—Å—Ç–æ—è–Ω–∏—è —Ü–∏—Å—Ç–µ—Ä–Ω—ã
        if –Ω–µ —Ü–∏—Å—Ç–µ—Ä–Ω–∞.–≤–∫–ª—é—á–µ–Ω–∞:
            print(f"\n–û–®–ò–ë–ö–ê: –¶–∏—Å—Ç–µ—Ä–Ω–∞ {id_—Ü–∏—Å—Ç–µ—Ä–Ω—ã} –æ—Ç–∫–ª—é—á–µ–Ω–∞!")
            if —Ü–∏—Å—Ç–µ—Ä–Ω–∞.–∞–≤—Ç–æ–æ—Ç–∫–ª:
                print("–ü—Ä–∏—á–∏–Ω–∞: –Ω–∏–∑–∫–∏–π —É—Ä–æ–≤–µ–Ω—å —Ç–æ–ø–ª–∏–≤–∞")
            print("–û—Ç–ø—É—Å–∫ —Ç–æ–ø–ª–∏–≤–∞ –Ω–µ–≤–æ–∑–º–æ–∂–µ–Ω.")
            input("\n–ù–∞–∂–º–∏—Ç–µ Enter...")
            return
        
        # 3. –í–≤–æ–¥ –∫–æ–ª–∏—á–µ—Å—Ç–≤–∞ –ª–∏—Ç—Ä–æ–≤
        print(f"\n–¢–µ–∫—É—â–∏–π –æ–±—ä–µ–º –≤ —Ü–∏—Å—Ç–µ—Ä–Ω–µ: {—Ü–∏—Å—Ç–µ—Ä–Ω–∞.—Ç–µ–∫—É—â–∏–π:.0f} –ª")
        –ª–∏—Ç—Ä—ã, –æ—à–∏–±–∫–∞ = –ø—Ä–æ–≤–µ—Ä–∏—Ç—å_–≤–≤–æ–¥_—á–∏—Å–ª–∞(input("–í–≤–µ–¥–∏—Ç–µ –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ –ª–∏—Ç—Ä–æ–≤: "), 0.1, —Ü–∏—Å—Ç–µ—Ä–Ω–∞.—Ç–µ–∫—É—â–∏–π)
        if –æ—à–∏–±–∫–∞:
            print(f"–û—à–∏–±–∫–∞: {–æ—à–∏–±–∫–∞}")
            input("\n–ù–∞–∂–º–∏—Ç–µ Enter...")
            return
        
        # 4. –†–∞—Å—á–µ—Ç —Å—Ç–æ–∏–º–æ—Å—Ç–∏
        —Ü–µ–Ω–∞ = –¶–ï–ù–´_–¢–û–ü–õ–ò–í–ê[—Ç–∏–ø_—Ç–æ–ø–ª–∏–≤–∞]
        —Å—Ç–æ–∏–º–æ—Å—Ç—å = –ª–∏—Ç—Ä—ã * —Ü–µ–Ω–∞
        
        print(f"\n" + "-"*30)
        print(f"–ò–¢–û–ì–û –ö –û–ü–õ–ê–¢–ï:")
        print(f"{–ª–∏—Ç—Ä—ã:.1f} –ª √ó {—Ü–µ–Ω–∞:.2f} ‚ÇΩ = {—Å—Ç–æ–∏–º–æ—Å—Ç—å:.2f} ‚ÇΩ")
        print("-"*30)
        
        # 5. –ü–æ–¥—Ç–≤–µ—Ä–∂–¥–µ–Ω–∏–µ –æ–ø–ª–∞—Ç—ã
        –ø–æ–¥—Ç–≤–µ—Ä–¥–∏—Ç—å = input("\n–ü–æ–¥—Ç–≤–µ—Ä–¥–∏—Ç—å –æ–ø–ª–∞—Ç—É? (–¥/–Ω): ").lower()
        if –ø–æ–¥—Ç–≤–µ—Ä–¥–∏—Ç—å != '–¥':
            print("\n–û–ø–µ—Ä–∞—Ü–∏—è –æ—Ç–º–µ–Ω–µ–Ω–∞.")
            input("\n–ù–∞–∂–º–∏—Ç–µ Enter...")
            return
        
        # 6. –í—ã–ø–æ–ª–Ω–µ–Ω–∏–µ –æ–ø–µ—Ä–∞—Ü–∏–∏
        —É—Å–ø–µ—Ö, —Å–æ–æ–±—â–µ–Ω–∏–µ = —Ü–∏—Å—Ç–µ—Ä–Ω–∞.–æ—Ç–ø—É—Å—Ç–∏—Ç—å_—Ç–æ–ø–ª–∏–≤–æ(–ª–∏—Ç—Ä—ã)
        if –Ω–µ —É—Å–ø–µ—Ö:
            print(f"\n–û–®–ò–ë–ö–ê: {—Å–æ–æ–±—â–µ–Ω–∏–µ}")
            input("\n–ù–∞–∂–º–∏—Ç–µ Enter...")
            return
        
        # 7. –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫–∏
        self.—Å—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞["–≤—Å–µ–≥–æ_–º–∞—à–∏–Ω"] += 1
        self.—Å—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞["–æ–±—â–∏–π_–¥–æ—Ö–æ–¥"] += —Å—Ç–æ–∏–º–æ—Å—Ç—å
        
        if —Ç–∏–ø_—Ç–æ–ø–ª–∏–≤–∞ in self.—Å—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞["–ø–æ_—Ç–∏–ø–∞–º"]:
            –¥–∞–Ω–Ω—ã–µ = self.—Å—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞["–ø–æ_—Ç–∏–ø–∞–º"][—Ç–∏–ø_—Ç–æ–ø–ª–∏–≤–∞]
            –¥–∞–Ω–Ω—ã–µ["–ª–∏—Ç—Ä–æ–≤"] += –ª–∏—Ç—Ä—ã
            –¥–∞–Ω–Ω—ã–µ["–¥–æ—Ö–æ–¥"] += —Å—Ç–æ–∏–º–æ—Å—Ç—å
            –¥–∞–Ω–Ω—ã–µ["—Ç—Ä–∞–Ω–∑–∞–∫—Ü–∏–π"] += 1
        
        # 8. –ó–∞–ø–∏—Å—å –≤ –∏—Å—Ç–æ—Ä–∏—é
        self.–¥–æ–±–∞–≤–∏—Ç—å_–≤_–∏—Å—Ç–æ—Ä–∏—é(
            "–ø—Ä–æ–¥–∞–∂–∞",
            f"–ü—Ä–æ–¥–∞–∂–∞ {–ª–∏—Ç—Ä—ã:.1f} –ª {—Ç–∏–ø_—Ç–æ–ø–ª–∏–≤–∞} –Ω–∞ –∫–æ–ª–æ–Ω–∫–µ {–∫–æ–ª–æ–Ω–∫–∞}",
            {
                "–∫–æ–ª–æ–Ω–∫–∞": –∫–æ–ª–æ–Ω–∫–∞,
                "—Ç–æ–ø–ª–∏–≤–æ": —Ç–∏–ø_—Ç–æ–ø–ª–∏–≤–∞,
                "–ª–∏—Ç—Ä—ã": –ª–∏—Ç—Ä—ã,
                "—Ü–µ–Ω–∞": —Ü–µ–Ω–∞,
                "—Å—Ç–æ–∏–º–æ—Å—Ç—å": —Å—Ç–æ–∏–º–æ—Å—Ç—å,
                "—Ü–∏—Å—Ç–µ—Ä–Ω–∞": id_—Ü–∏—Å—Ç–µ—Ä–Ω—ã
            }
        )
        
        # 9. –°–æ—Ö—Ä–∞–Ω–µ–Ω–∏–µ –¥–∞–Ω–Ω—ã—Ö
        self.—Å–æ—Ö—Ä–∞–Ω–∏—Ç—å_–≤—Å–µ_–¥–∞–Ω–Ω—ã–µ()
        
        # 10. –í—ã–≤–æ–¥ —Ä–µ–∑—É–ª—å—Ç–∞—Ç–∞
        print("\n" + "="*50)
        print("–û–ü–ï–†–ê–¶–ò–Ø –í–´–ü–û–õ–ù–ï–ù–ê –£–°–ü–ï–®–ù–û!")
        print("="*50)
        print(f"\n–û—Ç–ø—É—â–µ–Ω–æ: {–ª–∏—Ç—Ä—ã:.1f} –ª {—Ç–∏–ø_—Ç–æ–ø–ª–∏–≤–∞}")
        print(f"–°—Ç–æ–∏–º–æ—Å—Ç—å: {—Å—Ç–æ–∏–º–æ—Å—Ç—å:.2f} ‚ÇΩ")
        print(f"–û—Å—Ç–∞—Ç–æ–∫ –≤ —Ü–∏—Å—Ç–µ—Ä–Ω–µ: {—Ü–∏—Å—Ç–µ—Ä–Ω–∞.—Ç–µ–∫—É—â–∏–π:.0f} –ª")
        print(f"\n–°–ø–∞—Å–∏–±–æ –∑–∞ –ø–æ–∫—É–ø–∫—É!")
        
        input("\n–ù–∞–∂–º–∏—Ç–µ Enter –¥–ª—è –≤–æ–∑–≤—Ä–∞—Ç–∞...")
    
    def –ø–æ–∫–∞–∑–∞—Ç—å_—Ü–∏—Å—Ç–µ—Ä–Ω—ã(self):
        """–ü–æ–∫–∞–∑—ã–≤–∞–µ—Ç —Å–æ—Å—Ç–æ—è–Ω–∏–µ –≤—Å–µ—Ö —Ü–∏—Å—Ç–µ—Ä–Ω"""
        print("\n" + "="*50)
        print("–°–û–°–¢–û–Ø–ù–ò–ï –¶–ò–°–¢–ï–†–ù")
        print("="*50)
        
        # –ì—Ä—É–ø–ø–∏—Ä—É–µ–º –ø–æ —Ç–∏–ø—É —Ç–æ–ø–ª–∏–≤–∞
        —Ü–∏—Å—Ç–µ—Ä–Ω—ã_–ø–æ_—Ç–∏–ø—É = {}
        for —Ü–∏—Å—Ç–µ—Ä–Ω–∞ in self.—Ü–∏—Å—Ç–µ—Ä–Ω—ã.values():
            —Ç–∏–ø = —Ü–∏—Å—Ç–µ—Ä–Ω–∞.—Ç–∏–ø
            –µ—Å–ª–∏ —Ç–∏–ø not in —Ü–∏—Å—Ç–µ—Ä–Ω—ã_–ø–æ_—Ç–∏–ø—É:
                —Ü–∏—Å—Ç–µ—Ä–Ω—ã_–ø–æ_—Ç–∏–ø—É[—Ç–∏–ø] = []
            —Ü–∏—Å—Ç–µ—Ä–Ω—ã_–ø–æ_—Ç–∏–ø—É[—Ç–∏–ø].append(—Ü–∏—Å—Ç–µ—Ä–Ω–∞)
        
        for —Ç–∏–ø, —Å–ø–∏—Å–æ–∫_—Ü–∏—Å—Ç–µ—Ä–Ω in —Ü–∏—Å—Ç–µ—Ä–Ω—ã_–ø–æ_—Ç–∏–ø—É.items():
            print(f"\n{—Ç–∏–ø}:")
            
            for —Ü–∏—Å—Ç–µ—Ä–Ω–∞ in —Å–ø–∏—Å–æ–∫_—Ü–∏—Å—Ç–µ—Ä–Ω:
                –ø—Ä–æ—Ü–µ–Ω—Ç = —Ü–∏—Å—Ç–µ—Ä–Ω–∞.–ø–æ–ª—É—á–∏—Ç—å_–ø—Ä–æ—Ü–µ–Ω—Ç()
                —Å—Ç–∞—Ç—É—Å = —Ü–∏—Å—Ç–µ—Ä–Ω–∞.–ø–æ–ª—É—á–∏—Ç—å_—Å—Ç–∞—Ç—É—Å()
                
                # –û–ø—Ä–µ–¥–µ–ª—è–µ–º —Ü–≤–µ—Ç —Å—Ç–∞—Ç—É—Å–∞ –¥–ª—è –Ω–∞–≥–ª—è–¥–Ω–æ—Å—Ç–∏
                –µ—Å–ª–∏ —Ü–∏—Å—Ç–µ—Ä–Ω–∞.–≤–∫–ª—é—á–µ–Ω–∞:
                    –µ—Å–ª–∏ –ø—Ä–æ—Ü–µ–Ω—Ç > 20:
                        —Å—Ç–∞—Ç—É—Å_—Ç–µ–∫—Å—Ç = f"[–í–ö–õ]"
                    –∏–Ω–∞—á–µ:
                        —Å—Ç–∞—Ç—É—Å_—Ç–µ–∫—Å—Ç = f"[–í–ö–õ] ‚ö† –Ω–∏–∑–∫–∏–π —É—Ä–æ–≤–µ–Ω—å"
                –∏–Ω–∞—á–µ:
                    –µ—Å–ª–∏ —Ü–∏—Å—Ç–µ—Ä–Ω–∞.–∞–≤—Ç–æ–æ—Ç–∫–ª:
                        —Å—Ç–∞—Ç—É—Å_—Ç–µ–∫—Å—Ç = f"[–í–´–ö–õ] üî¥ –Ω–∏–∑–∫–∏–π —É—Ä–æ–≤–µ–Ω—å"
                    –∏–Ω–∞—á–µ:
                        —Å—Ç–∞—Ç—É—Å_—Ç–µ–∫—Å—Ç = f"[–í–´–ö–õ]"
                
                print(f"  {—Ü–∏—Å—Ç–µ—Ä–Ω–∞.–∏–¥}")
                print(f"    –û–±—ä–µ–º: {—Ü–∏—Å—Ç–µ—Ä–Ω–∞.—Ç–µ–∫—É—â–∏–π:.0f} / {—Ü–∏—Å—Ç–µ—Ä–Ω–∞.–º–∞–∫—Å:.0f} –ª ({–ø—Ä–æ—Ü–µ–Ω—Ç:.1f}%)")
                print(f"    –°—Ç–∞—Ç—É—Å: {—Å—Ç–∞—Ç—É—Å_—Ç–µ–∫—Å—Ç}")
        
        input("\n–ù–∞–∂–º–∏—Ç–µ Enter...")
    
    def –ø–æ–ø–æ–ª–Ω–∏—Ç—å_—Ü–∏—Å—Ç–µ—Ä–Ω—É(self):
        """–ü–æ–ø–æ–ª–Ω—è–µ—Ç –≤—ã–±—Ä–∞–Ω–Ω—É—é —Ü–∏—Å—Ç–µ—Ä–Ω—É"""
        –µ—Å–ª–∏ self.–∞–≤–∞—Ä–∏—è["–∞–∫—Ç–∏–≤–Ω–∞"]:
            print("\n–û–®–ò–ë–ö–ê: –ê–∫—Ç–∏–≤–µ–Ω –∞–≤–∞—Ä–∏–π–Ω—ã–π —Ä–µ–∂–∏–º!")
            print("–ü–æ–ø–æ–ª–Ω–µ–Ω–∏–µ –Ω–µ–≤–æ–∑–º–æ–∂–Ω–æ.")
            input("\n–ù–∞–∂–º–∏—Ç–µ Enter...")
            return
        
        print("\n" + "="*50)
        print("–ü–û–ü–û–õ–ù–ï–ù–ò–ï –¶–ò–°–¢–ï–†–ù–´")
        print("="*50)
        
        # –í—ã–±–∏—Ä–∞–µ–º —Ü–∏—Å—Ç–µ—Ä–Ω—É
        print("\n–î–æ—Å—Ç—É–ø–Ω—ã–µ —Ü–∏—Å—Ç–µ—Ä–Ω—ã:")
        —Å–ø–∏—Å–æ–∫_—Ü–∏—Å—Ç–µ—Ä–Ω = list(self.—Ü–∏—Å—Ç–µ—Ä–Ω—ã.values())
        
        for i, —Ü–∏—Å—Ç–µ—Ä–Ω–∞ in enumerate(—Å–ø–∏—Å–æ–∫_—Ü–∏—Å—Ç–µ—Ä–Ω, 1):
            —Å–≤–æ–±–æ–¥–Ω–æ = —Ü–∏—Å—Ç–µ—Ä–Ω–∞.–º–∞–∫—Å - —Ü–∏—Å—Ç–µ—Ä–Ω–∞.—Ç–µ–∫—É—â–∏–π
            print(f"{i}) {—Ü–∏—Å—Ç–µ—Ä–Ω–∞.–∏–¥} ({—Ü–∏—Å—Ç–µ—Ä–Ω–∞.—Ç–∏–ø}): {—Ü–∏—Å—Ç–µ—Ä–Ω–∞.—Ç–µ–∫—É—â–∏–π:.0f} –ª (–º–æ–∂–Ω–æ –¥–æ–±–∞–≤–∏—Ç—å {—Å–≤–æ–±–æ–¥–Ω–æ:.0f} –ª)")
        
        –≤—ã–±–æ—Ä, –æ—à–∏–±–∫–∞ = –ø—Ä–æ–≤–µ—Ä–∏—Ç—å_–≤–≤–æ–¥_—Ü–µ–ª–æ–≥–æ(input("\n–í—ã–±–µ—Ä–∏—Ç–µ —Ü–∏—Å—Ç–µ—Ä–Ω—É: "), 1, len(—Å–ø–∏—Å–æ–∫_—Ü–∏—Å—Ç–µ—Ä–Ω))
        –µ—Å–ª–∏ –æ—à–∏–±–∫–∞:
            print(f"–û—à–∏–±–∫–∞: {–æ—à–∏–±–∫–∞}")
            input("\n–ù–∞–∂–º–∏—Ç–µ Enter...")
            return
        
        —Ü–∏—Å—Ç–µ—Ä–Ω–∞ = —Å–ø–∏—Å–æ–∫_—Ü–∏—Å—Ç–µ—Ä–Ω[–≤—ã–±–æ—Ä - 1]
        —Å–≤–æ–±–æ–¥–Ω–æ = —Ü–∏—Å—Ç–µ—Ä–Ω–∞.–º–∞–∫—Å - —Ü–∏—Å—Ç–µ—Ä–Ω–∞.—Ç–µ–∫—É—â–∏–π
        
        print(f"\n–í—ã–±—Ä–∞–Ω–∞ —Ü–∏—Å—Ç–µ—Ä–Ω–∞: {—Ü–∏—Å—Ç–µ—Ä–Ω–∞.–∏–¥}")
        print(f"–¢–µ–∫—É—â–∏–π –æ–±—ä–µ–º: {—Ü–∏—Å—Ç–µ—Ä–Ω–∞.—Ç–µ–∫—É—â–∏–π:.0f} –ª")
        print(f"–ú–∞–∫—Å–∏–º–∞–ª—å–Ω—ã–π –æ–±—ä–µ–º: {—Ü–∏—Å—Ç–µ—Ä–Ω–∞.–º–∞–∫—Å:.0f} –ª")
        print(f"–ú–æ–∂–Ω–æ –¥–æ–±–∞–≤–∏—Ç—å: {—Å–≤–æ–±–æ–¥–Ω–æ:.0f} –ª")
        
        # –í–≤–æ–¥ –∫–æ–ª–∏—á–µ—Å—Ç–≤–∞
        –ª–∏—Ç—Ä—ã, –æ—à–∏–±–∫–∞ = –ø—Ä–æ–≤–µ—Ä–∏—Ç—å_–≤–≤–æ–¥_—á–∏—Å–ª–∞(input("\n–°–∫–æ–ª—å–∫–æ –ª–∏—Ç—Ä–æ–≤ –¥–æ–±–∞–≤–∏—Ç—å? "), 0.1, —Å–≤–æ–±–æ–¥–Ω–æ)
        –µ—Å–ª–∏ –æ—à–∏–±–∫–∞:
            print(f"–û—à–∏–±–∫–∞: {–æ—à–∏–±–∫–∞}")
            input("\n–ù–∞–∂–º–∏—Ç–µ Enter...")
            return
        
        # –ü–æ–¥—Ç–≤–µ—Ä–∂–¥–µ–Ω–∏–µ
        print(f"\n–í—ã —Å–æ–±–∏—Ä–∞–µ—Ç–µ—Å—å –¥–æ–±–∞–≤–∏—Ç—å {–ª–∏—Ç—Ä—ã:.0f} –ª –≤ —Ü–∏—Å—Ç–µ—Ä–Ω—É {—Ü–∏—Å—Ç–µ—Ä–Ω–∞.–∏–¥}")
        –ø–æ–¥—Ç–≤–µ—Ä–¥–∏—Ç—å = input("–ü–æ–¥—Ç–≤–µ—Ä–¥–∏—Ç—å? (–¥/–Ω): ").lower()
        –µ—Å–ª–∏ –ø–æ–¥—Ç–≤–µ—Ä–¥–∏—Ç—å != '–¥':
            print("\n–û–ø–µ—Ä–∞—Ü–∏—è –æ—Ç–º–µ–Ω–µ–Ω–∞.")
            input("\n–ù–∞–∂–º–∏—Ç–µ Enter...")
            return
        
        # –ü–æ–ø–æ–ª–Ω–µ–Ω–∏–µ
        —É—Å–ø–µ—Ö, —Å–æ–æ–±—â–µ–Ω–∏–µ = —Ü–∏—Å—Ç–µ—Ä–Ω–∞.–ø–æ–ø–æ–ª–Ω–∏—Ç—å(–ª–∏—Ç—Ä—ã)
        –µ—Å–ª–∏ –Ω–µ —É—Å–ø–µ—Ö:
            print(f"\n–û–®–ò–ë–ö–ê: {—Å–æ–æ–±—â–µ–Ω–∏–µ}")
            input("\n–ù–∞–∂–º–∏—Ç–µ Enter...")
            return
        
        # –ó–∞–ø–∏—Å—å –≤ –∏—Å—Ç–æ—Ä–∏—é
        self.–¥–æ–±–∞–≤–∏—Ç—å_–≤_–∏—Å—Ç–æ—Ä–∏—é(
            "–ø–æ–ø–æ–ª–Ω–µ–Ω–∏–µ",
            f"–ü–æ–ø–æ–ª–Ω–µ–Ω–∏–µ —Ü–∏—Å—Ç–µ—Ä–Ω—ã {—Ü–∏—Å—Ç–µ—Ä–Ω–∞.–∏–¥} –Ω–∞ {–ª–∏—Ç—Ä—ã:.0f} –ª",
            {
                "—Ü–∏—Å—Ç–µ—Ä–Ω–∞": —Ü–∏—Å—Ç–µ—Ä–Ω–∞.–∏–¥,
                "–ª–∏—Ç—Ä—ã": –ª–∏—Ç—Ä—ã,
                "–Ω–æ–≤—ã–π_–æ–±—ä–µ–º": —Ü–∏—Å—Ç–µ—Ä–Ω–∞.—Ç–µ–∫—É—â–∏–π
            }
        )
        
        # –°–æ—Ö—Ä–∞–Ω–µ–Ω–∏–µ
        self.—Å–æ—Ö—Ä–∞–Ω–∏—Ç—å_–≤—Å–µ_–¥–∞–Ω–Ω—ã–µ()
        
        print(f"\n‚úÖ –¶–∏—Å—Ç–µ—Ä–Ω–∞ —É—Å–ø–µ—à–Ω–æ –ø–æ–ø–æ–ª–Ω–µ–Ω–∞!")
        print(f"–ù–æ–≤—ã–π –æ–±—ä–µ–º: {—Ü–∏—Å—Ç–µ—Ä–Ω–∞.—Ç–µ–∫—É—â–∏–π:.0f} –ª")
        print("\n–í–ê–ñ–ù–û: –¶–∏—Å—Ç–µ—Ä–Ω–∞ –æ—Å—Ç–∞–ª–∞—Å—å –≤ —Ç–µ–∫—É—â–µ–º —Å–æ—Å—Ç–æ—è–Ω–∏–∏.")
        print("–î–ª—è –≤–∫–ª—é—á–µ–Ω–∏—è –∏—Å–ø–æ–ª—å–∑—É–π—Ç–µ –º–µ–Ω—é —É–ø—Ä–∞–≤–ª–µ–Ω–∏—è —Ü–∏—Å—Ç–µ—Ä–Ω–∞–º–∏.")
        
        input("\n–ù–∞–∂–º–∏—Ç–µ Enter...")
    
    def –ø–æ–∫–∞–∑–∞—Ç—å_—Å—Ç–∞—Ç–∏—Å—Ç–∏–∫—É(self):
        """–ü–æ–∫–∞–∑—ã–≤–∞–µ—Ç —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫—É —Ä–∞–±–æ—Ç—ã –ê–ó–°"""
        print("\n" + "="*50)
        print("–°–¢–ê–¢–ò–°–¢–ò–ö–ê –†–ê–ë–û–¢–´ –ê–ó–°")
        print("="*50)
        
        –≤—Å–µ–≥–æ_–º–∞—à–∏–Ω = self.—Å—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞["–≤—Å–µ–≥–æ_–º–∞—à–∏–Ω"]
        –æ–±—â–∏–π_–¥–æ—Ö–æ–¥ = self.—Å—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞["–æ–±—â–∏–π_–¥–æ—Ö–æ–¥"]
        
        print(f"\n–û–±—Å–ª—É–∂–µ–Ω–æ –∞–≤—Ç–æ–º–æ–±–∏–ª–µ–π: {–≤—Å–µ–≥–æ_–º–∞—à–∏–Ω:,}")
        print(f"–û–±—â–∏–π –¥–æ—Ö–æ–¥: {–æ–±—â–∏–π_–¥–æ—Ö–æ–¥:,.2f} ‚ÇΩ")
        
        print("\n–ü—Ä–æ–¥–∞–∂–∏ –ø–æ –≤–∏–¥–∞–º —Ç–æ–ø–ª–∏–≤–∞:")
        print("-"*40)
        
        for —Ç–∏–ø_—Ç–æ–ø–ª–∏–≤–∞ in ["–ê–ò-92", "–ê–ò-95", "–ê–ò-98", "–î–¢"]:
            –µ—Å–ª–∏ —Ç–∏–ø_—Ç–æ–ø–ª–∏–≤–∞ in self.—Å—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞["–ø–æ_—Ç–∏–ø–∞–º"]:
                –¥–∞–Ω–Ω—ã–µ = self.—Å—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞["–ø–æ_—Ç–∏–ø–∞–º"][—Ç–∏–ø_—Ç–æ–ø–ª–∏–≤–∞]
                –ª–∏—Ç—Ä–æ–≤ = –¥–∞–Ω–Ω—ã–µ["–ª–∏—Ç—Ä–æ–≤"]
                –¥–æ—Ö–æ–¥ = –¥–∞–Ω–Ω—ã–µ["–¥–æ—Ö–æ–¥"]
                —Ç—Ä–∞–Ω–∑–∞–∫—Ü–∏–π = –¥–∞–Ω–Ω—ã–µ["—Ç—Ä–∞–Ω–∑–∞–∫—Ü–∏–π"]
                
                –µ—Å–ª–∏ –ª–∏—Ç—Ä–æ–≤ > 0:
                    print(f"\n{—Ç–∏–ø_—Ç–æ–ø–ª–∏–≤–∞}:")
                    print(f"  –ü—Ä–æ–¥–∞–Ω–æ: {–ª–∏—Ç—Ä–æ–≤:.0f} –ª")
                    print(f"  –î–æ—Ö–æ–¥: {–¥–æ—Ö–æ–¥:,.0f} ‚ÇΩ")
                    print(f"  –¢—Ä–∞–Ω–∑–∞–∫—Ü–∏–π: {—Ç—Ä–∞–Ω–∑–∞–∫—Ü–∏–π}")
        
        # –¢–µ–∫—É—â–∏–µ –æ—Å—Ç–∞—Ç–∫–∏ –∏ –∏—Ö —Å—Ç–æ–∏–º–æ—Å—Ç—å
        print("\n" + "="*50)
        print("–¢–ï–ö–£–©–ò–ï –û–°–¢–ê–¢–ö–ò")
        print("="*50)
        
        –æ–±—â–∞—è_—Å—Ç–æ–∏–º–æ—Å—Ç—å = 0
        
        –¥–ª—è —Ü–∏—Å—Ç–µ—Ä–Ω–∞ –≤ self.—Ü–∏—Å—Ç–µ—Ä–Ω—ã.values():
            —Ü–µ–Ω–∞ = –¶–ï–ù–´_–¢–û–ü–õ–ò–í–ê.get(—Ü–∏—Å—Ç–µ—Ä–Ω–∞.—Ç–∏–ø, 50.0)
            —Å—Ç–æ–∏–º–æ—Å—Ç—å = —Ü–∏—Å—Ç–µ—Ä–Ω–∞.—Ç–µ–∫—É—â–∏–π * —Ü–µ–Ω–∞
            –æ–±—â–∞—è_—Å—Ç–æ–∏–º–æ—Å—Ç—å += —Å—Ç–æ–∏–º–æ—Å—Ç—å
            
            print(f"\n{—Ü–∏—Å—Ç–µ—Ä–Ω–∞.–∏–¥:12} ({—Ü–∏—Å—Ç–µ—Ä–Ω–∞.—Ç–∏–ø:6}):")
            print(f"  –û—Å—Ç–∞—Ç–æ–∫: {—Ü–∏—Å—Ç–µ—Ä–Ω–∞.—Ç–µ–∫—É—â–∏–π:7.0f} –ª")
            print(f"  –°—Ç–æ–∏–º–æ—Å—Ç—å: {—Å—Ç–æ–∏–º–æ—Å—Ç—å:10,.0f} ‚ÇΩ")
        
        print(f"\n{'='*50}")
        print(f"–û–±—â–∞—è —Å—Ç–æ–∏–º–æ—Å—Ç—å –æ—Å—Ç–∞—Ç–∫–æ–≤: {–æ–±—â–∞—è_—Å—Ç–æ–∏–º–æ—Å—Ç—å:,.0f} ‚ÇΩ")
        
        input("\n–ù–∞–∂–º–∏—Ç–µ Enter...")
    
    def –ø–æ–∫–∞–∑–∞—Ç—å_–∏—Å—Ç–æ—Ä–∏—é(self):
        """–ü–æ–∫–∞–∑—ã–≤–∞–µ—Ç –∏—Å—Ç–æ—Ä–∏—é –æ–ø–µ—Ä–∞—Ü–∏–π"""
        print("\n" + "="*50)
        print("–ò–°–¢–û–†–ò–Ø –û–ü–ï–†–ê–¶–ò–ô")
        print("="*50)
        
        –µ—Å–ª–∏ –Ω–µ self.–∏—Å—Ç–æ—Ä–∏—è:
            print("\n–ò—Å—Ç–æ—Ä–∏—è –æ–ø–µ—Ä–∞—Ü–∏–π –ø—É—Å—Ç–∞.")
            input("\n–ù–∞–∂–º–∏—Ç–µ Enter...")
            return
        
        # –ë–µ—Ä–µ–º –ø–æ—Å–ª–µ–¥–Ω–∏–µ 15 –æ–ø–µ—Ä–∞—Ü–∏–π
        –ø–æ—Å–ª–µ–¥–Ω–∏–µ = self.–∏—Å—Ç–æ—Ä–∏—è[-15:] –µ—Å–ª–∏ len(self.–∏—Å—Ç–æ—Ä–∏—è) > 15 else self.–∏—Å—Ç–æ—Ä–∏—è
        
        print(f"\n–ü–æ—Å–ª–µ–¥–Ω–∏–µ {len(–ø–æ—Å–ª–µ–¥–Ω–∏–µ)} –æ–ø–µ—Ä–∞—Ü–∏–π:")
        print("-"*60)
        
        –¥–ª—è i, –∑–∞–ø–∏—Å—å –≤ enumerate(reversed(–ø–æ—Å–ª–µ–¥–Ω–∏–µ), 1):
            print(f"\n{i:2}. [{–∑–∞–ø–∏—Å—å['–≤—Ä–µ–º—è']}]")
            print(f"    –¢–∏–ø: {–∑–∞–ø–∏—Å—å['—Ç–∏–ø'].upper()}")
            print(f"    {–∑–∞–ø–∏—Å—å['–æ–ø–∏—Å–∞–Ω–∏–µ']}")
            
            –µ—Å–ª–∏ –∑–∞–ø–∏—Å—å['–¥–µ—Ç–∞–ª–∏']:
                print(f"    –î–µ—Ç–∞–ª–∏:")
                –¥–ª—è –∫–ª—é—á, –∑–Ω–∞—á–µ–Ω–∏–µ –≤ –∑–∞–ø–∏—Å—å['–¥–µ—Ç–∞–ª–∏'].items():
                    –µ—Å–ª–∏ isinstance(–∑–Ω–∞—á–µ–Ω–∏–µ, float):
                        print(f"      {–∫–ª—é—á}: {–∑–Ω–∞—á–µ–Ω–∏–µ:.2f}")
                    else:
                        print(f"      {–∫–ª—é—á}: {–∑–Ω–∞—á–µ–Ω–∏–µ}")
        
        input("\n–ù–∞–∂–º–∏—Ç–µ Enter...")
    
    def –ø–µ—Ä–µ–∫–∞—á–∞—Ç—å_—Ç–æ–ø–ª–∏–≤–æ(self):
        """–ü–µ—Ä–µ–∫–∞—á–∏–≤–∞–µ—Ç —Ç–æ–ø–ª–∏–≤–æ –º–µ–∂–¥—É —Ü–∏—Å—Ç–µ—Ä–Ω–∞–º–∏ –æ–¥–Ω–æ–≥–æ —Ç–∏–ø–∞"""
        –µ—Å–ª–∏ self.–∞–≤–∞—Ä–∏—è["–∞–∫—Ç–∏–≤–Ω–∞"]:
            print("\n–û–®–ò–ë–ö–ê: –ê–∫—Ç–∏–≤–µ–Ω –∞–≤–∞—Ä–∏–π–Ω—ã–π —Ä–µ–∂–∏–º!")
            print("–ü–µ—Ä–µ–∫–∞—á–∫–∞ –Ω–µ–≤–æ–∑–º–æ–∂–Ω–∞.")
            input("\n–ù–∞–∂–º–∏—Ç–µ Enter...")
            return
        
        print("\n" + "="*50)
        print("–ü–ï–†–ï–ö–ê–ß–ö–ê –¢–û–ü–õ–ò–í–ê")
        print("="*50)
        print("–ü–µ—Ä–µ–∫–∞—á–∫–∞ –≤–æ–∑–º–æ–∂–Ω–∞ —Ç–æ–ª—å–∫–æ –º–µ–∂–¥—É —Ü–∏—Å—Ç–µ—Ä–Ω–∞–º–∏ –æ–¥–Ω–æ–≥–æ —Ç–∏–ø–∞ —Ç–æ–ø–ª–∏–≤–∞")
        
        # –ì—Ä—É–ø–ø–∏—Ä—É–µ–º —Ü–∏—Å—Ç–µ—Ä–Ω—ã –ø–æ —Ç–∏–ø—É
        —Ü–∏—Å—Ç–µ—Ä–Ω—ã_–ø–æ_—Ç–∏–ø—É = {}
        –¥–ª—è —Ü–∏—Å—Ç–µ—Ä–Ω–∞ –≤ self.—Ü–∏—Å—Ç–µ—Ä–Ω—ã.values():
            —Ç–∏–ø = —Ü–∏—Å—Ç–µ—Ä–Ω–∞.—Ç–∏–ø
            –µ—Å–ª–∏ —Ç–∏–ø not in —Ü–∏—Å—Ç–µ—Ä–Ω—ã_–ø–æ_—Ç–∏–ø—É:
                —Ü–∏—Å—Ç–µ—Ä–Ω—ã_–ø–æ_—Ç–∏–ø—É[—Ç–∏–ø] = []
            —Ü–∏—Å—Ç–µ—Ä–Ω—ã_–ø–æ_—Ç–∏–ø—É[—Ç–∏–ø].append(—Ü–∏—Å—Ç–µ—Ä–Ω–∞)
        
        # –í—ã–±–∏—Ä–∞–µ–º —Ç–∏–ø —Ç–æ–ø–ª–∏–≤–∞
        print("\n–í—ã–±–µ—Ä–∏—Ç–µ —Ç–∏–ø —Ç–æ–ø–ª–∏–≤–∞:")
        —Ç–∏–ø—ã = list(—Ü–∏—Å—Ç–µ—Ä–Ω—ã_–ø–æ_—Ç–∏–ø—É.keys())
        
        –¥–ª—è i, —Ç–∏–ø –≤ enumerate(—Ç–∏–ø—ã, 1):
            –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ = len(—Ü–∏—Å—Ç–µ—Ä–Ω—ã_–ø–æ_—Ç–∏–ø—É[—Ç–∏–ø])
            print(f"{i}) {—Ç–∏–ø} ({–∫–æ–ª–∏—á–µ—Å—Ç–≤–æ} —Ü–∏—Å—Ç–µ—Ä–Ω)")
        
        –≤—ã–±–æ—Ä, –æ—à–∏–±–∫–∞ = –ø—Ä–æ–≤–µ—Ä–∏—Ç—å_–≤–≤–æ–¥_—Ü–µ–ª–æ–≥–æ(input("\n> "), 1, len(—Ç–∏–ø—ã))
        –µ—Å–ª–∏ –æ—à–∏–±–∫–∞:
            print(f"–û—à–∏–±–∫–∞: {–æ—à–∏–±–∫–∞}")
            input("\n–ù–∞–∂–º–∏—Ç–µ Enter...")
            return
        
        –≤—ã–±—Ä–∞–Ω–Ω—ã–π_—Ç–∏–ø = —Ç–∏–ø—ã[–≤—ã–±–æ—Ä - 1]
        —Ü–∏—Å—Ç–µ—Ä–Ω—ã_—ç—Ç–æ–≥–æ_—Ç–∏–ø–∞ = —Ü–∏—Å—Ç–µ—Ä–Ω—ã_–ø–æ_—Ç–∏–ø—É[–≤—ã–±—Ä–∞–Ω–Ω—ã–π_—Ç–∏–ø]
        
        –µ—Å–ª–∏ len(—Ü–∏—Å—Ç–µ—Ä–Ω—ã_—ç—Ç–æ–≥–æ_—Ç–∏–ø–∞) < 2:
            print(f"\n–û–®–ò–ë–ö–ê: –î–ª—è —Ç–æ–ø–ª–∏–≤–∞ {–≤—ã–±—Ä–∞–Ω–Ω—ã–π_—Ç–∏–ø} —Ç–æ–ª—å–∫–æ –æ–¥–Ω–∞ —Ü–∏—Å—Ç–µ—Ä–Ω–∞.")
            print("–ü–µ—Ä–µ–∫–∞—á–∫–∞ –Ω–µ–≤–æ–∑–º–æ–∂–Ω–∞.")
            input("\n–ù–∞–∂–º–∏—Ç–µ Enter...")
            return
        
        # –í—ã–±–∏—Ä–∞–µ–º –∏—Å—Ç–æ—á–Ω–∏–∫
        print(f"\n–¶–∏—Å—Ç–µ—Ä–Ω—ã —Å {–≤—ã–±—Ä–∞–Ω–Ω—ã–π_—Ç–∏–ø}:")
        –¥–ª—è i, —Ü–∏—Å—Ç–µ—Ä–Ω–∞ –≤ enumerate(—Ü–∏—Å—Ç–µ—Ä–Ω—ã_—ç—Ç–æ–≥–æ_—Ç–∏–ø–∞, 1):
            —Å—Ç–∞—Ç—É—Å = "–í–ö–õ" –µ—Å–ª–∏ —Ü–∏—Å—Ç–µ—Ä–Ω–∞.–≤–∫–ª—é—á–µ–Ω–∞ else "–í–´–ö–õ"
            print(f"{i}) {—Ü–∏—Å—Ç–µ—Ä–Ω–∞.–∏–¥}: {—Ü–∏—Å—Ç–µ—Ä–Ω–∞.—Ç–µ–∫—É—â–∏–π:.0f} –ª [{—Å—Ç–∞—Ç—É—Å}]")
        
        –∏—Å—Ç–æ—á–Ω–∏–∫_–∏–Ω–¥–µ–∫—Å, –æ—à–∏–±–∫–∞ = –ø—Ä–æ–≤–µ—Ä–∏—Ç—å_–≤–≤–æ–¥_—Ü–µ–ª–æ–≥–æ(input("\n–í—ã–±–µ—Ä–∏—Ç–µ –∏—Å—Ç–æ—á–Ω–∏–∫ (–æ—Ç–∫—É–¥–∞): "), 1, len(—Ü–∏—Å—Ç–µ—Ä–Ω—ã_—ç—Ç–æ–≥–æ_—Ç–∏–ø–∞))
        –µ—Å–ª–∏ –æ—à–∏–±–∫–∞:
            print(f"–û—à–∏–±–∫–∞: {–æ—à–∏–±–∫–∞}")
            input("\n–ù–∞–∂–º–∏—Ç–µ Enter...")
            return
        
        –∏—Å—Ç–æ—á–Ω–∏–∫ = —Ü–∏—Å—Ç–µ—Ä–Ω—ã_—ç—Ç–æ–≥–æ_—Ç–∏–ø–∞[–∏—Å—Ç–æ—á–Ω–∏–∫_–∏–Ω–¥–µ–∫—Å - 1]
        
        # –ü—Ä–æ–≤–µ—Ä—è–µ–º –∏—Å—Ç–æ—á–Ω–∏–∫
        –µ—Å–ª–∏ –Ω–µ –∏—Å—Ç–æ—á–Ω–∏–∫.–≤–∫–ª—é—á–µ–Ω–∞:
            print(f"\n–û–®–ò–ë–ö–ê: –¶–∏—Å—Ç–µ—Ä–Ω–∞ {–∏—Å—Ç–æ—á–Ω–∏–∫.–∏–¥} –æ—Ç–∫–ª—é—á–µ–Ω–∞!")
            input("\n–ù–∞–∂–º–∏—Ç–µ Enter...")
            return
        
        # –í—ã–±–∏—Ä–∞–µ–º –ø—Ä–∏–µ–º–Ω–∏–∫
        print(f"\n–í—ã–±–µ—Ä–∏—Ç–µ –ø—Ä–∏–µ–º–Ω–∏–∫ (–∫—É–¥–∞), –∫—Ä–æ–º–µ {–∏—Å—Ç–æ—á–Ω–∏–∫.–∏–¥}:")
        –¥–æ—Å—Ç—É–ø–Ω—ã–µ_–ø—Ä–∏–µ–º–Ω–∏–∫–∏ = []
        
        –¥–ª—è i, —Ü–∏—Å—Ç–µ—Ä–Ω–∞ –≤ enumerate(—Ü–∏—Å—Ç–µ—Ä–Ω—ã_—ç—Ç–æ–≥–æ_—Ç–∏–ø–∞, 1):
            –µ—Å–ª–∏ —Ü–∏—Å—Ç–µ—Ä–Ω–∞.–∏–¥ != –∏—Å—Ç–æ—á–Ω–∏–∫.–∏–¥:
                —Å–≤–æ–±–æ–¥–Ω–æ = —Ü–∏—Å—Ç–µ—Ä–Ω–∞.–º–∞–∫—Å - —Ü–∏—Å—Ç–µ—Ä–Ω–∞.—Ç–µ–∫—É—â–∏–π
                print(f"{i}) {—Ü–∏—Å—Ç–µ—Ä–Ω–∞.–∏–¥}: {—Ü–∏—Å—Ç–µ—Ä–Ω–∞.—Ç–µ–∫—É—â–∏–π:.0f} –ª (—Å–≤–æ–±–æ–¥–Ω–æ {—Å–≤–æ–±–æ–¥–Ω–æ:.0f} –ª)")
                –¥–æ—Å—Ç—É–ø–Ω—ã–µ_–ø—Ä–∏–µ–º–Ω–∏–∫–∏.append((i, —Ü–∏—Å—Ç–µ—Ä–Ω–∞))
        
        –µ—Å–ª–∏ –Ω–µ –¥–æ—Å—Ç—É–ø–Ω—ã–µ_–ø—Ä–∏–µ–º–Ω–∏–∫–∏:
            print("–ù–µ—Ç –¥—Ä—É–≥–∏—Ö —Ü–∏—Å—Ç–µ—Ä–Ω —ç—Ç–æ–≥–æ —Ç–∏–ø–∞")
            input("\n–ù–∞–∂–º–∏—Ç–µ Enter...")
            return
        
        –Ω–æ–º–µ—Ä, –æ—à–∏–±–∫–∞ = –ø—Ä–æ–≤–µ—Ä–∏—Ç—å_–≤–≤–æ–¥_—Ü–µ–ª–æ–≥–æ(input("\n> "), 1, len(–¥–æ—Å—Ç—É–ø–Ω—ã–µ_–ø—Ä–∏–µ–º–Ω–∏–∫–∏))
        –µ—Å–ª–∏ –æ—à–∏–±–∫–∞:
            print(f"–û—à–∏–±–∫–∞: {–æ—à–∏–±–∫–∞}")
            input("\n–ù–∞–∂–º–∏—Ç–µ Enter...")
            return
        
        # –ù–∞—Ö–æ–¥–∏–º –ø—Ä–∏–µ–º–Ω–∏–∫ –ø–æ –Ω–æ–º–µ—Ä—É –≤ —Å–ø–∏—Å–∫–µ –¥–æ—Å—Ç—É–ø–Ω—ã—Ö
        –ø—Ä–∏–µ–º–Ω–∏–∫ = None
        –¥–ª—è i, —Ü–∏—Å—Ç–µ—Ä–Ω–∞ –≤ –¥–æ—Å—Ç—É–ø–Ω—ã–µ_–ø—Ä–∏–µ–º–Ω–∏–∫–∏:
            –µ—Å–ª–∏ i == –Ω–æ–º–µ—Ä:
                –ø—Ä–∏–µ–º–Ω–∏–∫ = —Ü–∏—Å—Ç–µ—Ä–Ω–∞
                break
        
        –µ—Å–ª–∏ –ø—Ä–∏–µ–º–Ω–∏–∫ is None:
            print("–û—à–∏–±–∫–∞ –≤—ã–±–æ—Ä–∞")
            input("\n–ù–∞–∂–º–∏—Ç–µ Enter...")
            return
        
        # –í–≤–æ–¥–∏–º –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ
        –º–æ–∂–Ω–æ_–ø–µ—Ä–µ–∫–∞—á–∞—Ç—å = min(–∏—Å—Ç–æ—á–Ω–∏–∫.—Ç–µ–∫—É—â–∏–π, –ø—Ä–∏–µ–º–Ω–∏–∫.–º–∞–∫—Å - –ø—Ä–∏–µ–º–Ω–∏–∫.—Ç–µ–∫—É—â–∏–π)
        print(f"\n–ú–æ–∂–Ω–æ –ø–µ—Ä–µ–∫–∞—á–∞—Ç—å –¥–æ {–º–æ–∂–Ω–æ_–ø–µ—Ä–µ–∫–∞—á–∞—Ç—å:.0f} –ª")
        print(f"–ò–∑ {–∏—Å—Ç–æ—á–Ω–∏–∫.–∏–¥}: {–∏—Å—Ç–æ—á–Ω–∏–∫.—Ç–µ–∫—É—â–∏–π:.0f} –ª")
        print(f"–í {–ø—Ä–∏–µ–º–Ω–∏–∫.–∏–¥}: —Å–≤–æ–±–æ–¥–Ω–æ {–ø—Ä–∏–µ–º–Ω–∏–∫.–º–∞–∫—Å - –ø—Ä–∏–µ–º–Ω–∏–∫.—Ç–µ–∫—É—â–∏–π:.0f} –ª")
        
        –ª–∏—Ç—Ä—ã, –æ—à–∏–±–∫–∞ = –ø—Ä–æ–≤–µ—Ä–∏—Ç—å_–≤–≤–æ–¥_—á–∏—Å–ª–∞(input("\n–°–∫–æ–ª—å–∫–æ –ª–∏—Ç—Ä–æ–≤ –ø–µ—Ä–µ–∫–∞—á–∞—Ç—å? "), 0.1, –º–æ–∂–Ω–æ_–ø–µ—Ä–µ–∫–∞—á–∞—Ç—å)
        –µ—Å–ª–∏ –æ—à–∏–±–∫–∞:
            print(f"–û—à–∏–±–∫–∞: {–æ—à–∏–±–∫–∞}")
            input("\n–ù–∞–∂–º–∏—Ç–µ Enter...")
            return
        
        # –ü–æ–¥—Ç–≤–µ—Ä–∂–¥–µ–Ω–∏–µ
        print(f"\n–í—ã —Å–æ–±–∏—Ä–∞–µ—Ç–µ—Å—å –ø–µ—Ä–µ–∫–∞—á–∞—Ç—å {–ª–∏—Ç—Ä—ã:.0f} –ª")
        print(f"–ò–∑: {–∏—Å—Ç–æ—á–Ω–∏–∫.–∏–¥}")
        print(f"–í: {–ø—Ä–∏–µ–º–Ω–∏–∫.–∏–¥}")
        
        –ø–æ–¥—Ç–≤–µ—Ä–¥–∏—Ç—å = input("\n–ü–æ–¥—Ç–≤–µ—Ä–¥–∏—Ç—å? (–¥/–Ω): ").lower()
        –µ—Å–ª–∏ –ø–æ–¥—Ç–≤–µ—Ä–¥–∏—Ç—å != '–¥':
            print("\n–û–ø–µ—Ä–∞—Ü–∏—è –æ—Ç–º–µ–Ω–µ–Ω–∞.")
            input("\n–ù–∞–∂–º–∏—Ç–µ Enter...")
            return
        
        # –í—ã–ø–æ–ª–Ω—è–µ–º –ø–µ—Ä–µ–∫–∞—á–∫—É
        —É—Å–ø–µ—Ö1, —Å–æ–æ–±—â–µ–Ω–∏–µ1 = –∏—Å—Ç–æ—á–Ω–∏–∫.–æ—Ç–ø—É—Å—Ç–∏—Ç—å_—Ç–æ–ø–ª–∏–≤–æ(–ª–∏—Ç—Ä—ã)
        –µ—Å–ª–∏ –Ω–µ —É—Å–ø–µ—Ö1:
            print(f"\n–û–®–ò–ë–ö–ê –ø—Ä–∏ –æ—Ç–ø—É—Å–∫–µ: {—Å–æ–æ–±—â–µ–Ω–∏–µ1}")
            input("\n–ù–∞–∂–º–∏—Ç–µ Enter...")
            return
        
        —É—Å–ø–µ—Ö2, —Å–æ–æ–±—â–µ–Ω–∏–µ2 = –ø—Ä–∏–µ–º–Ω–∏–∫.–ø–æ–ø–æ–ª–Ω–∏—Ç—å(–ª–∏—Ç—Ä—ã)
        –µ—Å–ª–∏ –Ω–µ —É—Å–ø–µ—Ö2:
            # –ï—Å–ª–∏ –Ω–µ –ø–æ–ª—É—á–∏–ª–æ—Å—å –¥–æ–±–∞–≤–∏—Ç—å –≤ –ø—Ä–∏–µ–º–Ω–∏–∫, –≤–æ–∑–≤—Ä–∞—â–∞–µ–º –≤ –∏—Å—Ç–æ—á–Ω–∏–∫
            –∏—Å—Ç–æ—á–Ω–∏–∫.–ø–æ–ø–æ–ª–Ω–∏—Ç—å(–ª–∏—Ç—Ä—ã)
            print(f"\n–û–®–ò–ë–ö–ê –ø—Ä–∏ –ø–æ–ø–æ–ª–Ω–µ–Ω–∏–∏: {—Å–æ–æ–±—â–µ–Ω–∏–µ2}")
            input("\n–ù–∞–∂–º–∏—Ç–µ Enter...")
            return
        
        # –ó–∞–ø–∏—Å—å –≤ –∏—Å—Ç–æ—Ä–∏—é
        self.–¥–æ–±–∞–≤–∏—Ç—å_–≤_–∏—Å—Ç–æ—Ä–∏—é(
            "–ø–µ—Ä–µ–∫–∞—á–∫–∞",
            f"–ü–µ—Ä–µ–∫–∞—á–∫–∞ {–ª–∏—Ç—Ä—ã:.0f} –ª {–≤—ã–±—Ä–∞–Ω–Ω—ã–π_—Ç–∏–ø} –∏–∑ {–∏—Å—Ç–æ—á–Ω–∏–∫.–∏–¥} –≤ {–ø—Ä–∏–µ–º–Ω–∏–∫.–∏–¥}",
            {
                "—Ç–æ–ø–ª–∏–≤–æ": –≤—ã–±—Ä–∞–Ω–Ω—ã–π_—Ç–∏–ø,
                "–∏—Å—Ç–æ—á–Ω–∏–∫": –∏—Å—Ç–æ—á–Ω–∏–∫.–∏–¥,
                "–ø—Ä–∏–µ–º–Ω–∏–∫": –ø—Ä–∏–µ–º–Ω–∏–∫.–∏–¥,
                "–ª–∏—Ç—Ä—ã": –ª–∏—Ç—Ä—ã
            }
        )
        
        # –°–æ—Ö—Ä–∞–Ω–µ–Ω–∏–µ
        self.—Å–æ—Ö—Ä–∞–Ω–∏—Ç—å_–≤—Å–µ_–¥–∞–Ω–Ω—ã–µ()
        
        print(f"\n‚úÖ –ü–µ—Ä–µ–∫–∞—á–∫–∞ –≤—ã–ø–æ–ª–Ω–µ–Ω–∞ —É—Å–ø–µ—à–Ω–æ!")
        print(f"{–∏—Å—Ç–æ—á–Ω–∏–∫.–∏–¥}: {–∏—Å—Ç–æ—á–Ω–∏–∫.—Ç–µ–∫—É—â–∏–π:.0f} –ª")
        print(f"{–ø—Ä–∏–µ–º–Ω–∏–∫.–∏–¥}: {–ø—Ä–∏–µ–º–Ω–∏–∫.—Ç–µ–∫—É—â–∏–π:.0f} –ª")
        
        input("\n–ù–∞–∂–º–∏—Ç–µ Enter...")
    
    def —É–ø—Ä–∞–≤–ª–µ–Ω–∏–µ_—Ü–∏—Å—Ç–µ—Ä–Ω–∞–º–∏(self):
        """–í–∫–ª—é—á–µ–Ω–∏–µ/–æ—Ç–∫–ª—é—á–µ–Ω–∏–µ —Ü–∏—Å—Ç–µ—Ä–Ω –≤—Ä—É—á–Ω—É—é"""
        print("\n" + "="*50)
        print("–£–ü–†–ê–í–õ–ï–ù–ò–ï –¶–ò–°–¢–ï–†–ù–ê–ú–ò")
        print("="*50)
        
        print("\n–í—ã–±–µ—Ä–∏—Ç–µ –¥–µ–π—Å—Ç–≤–∏–µ:")
        print("1) –í–∫–ª—é—á–∏—Ç—å —Ü–∏—Å—Ç–µ—Ä–Ω—É")
        print("2) –û—Ç–∫–ª—é—á–∏—Ç—å —Ü–∏—Å—Ç–µ—Ä–Ω—É")
        
        –¥–µ–π—Å—Ç–≤–∏–µ, –æ—à–∏–±–∫–∞ = –ø—Ä–æ–≤–µ—Ä–∏—Ç—å_–≤–≤–æ–¥_—Ü–µ–ª–æ–≥–æ(input("\n> "), 1, 2)
        –µ—Å–ª–∏ –æ—à–∏–±–∫–∞:
            print(f"–û—à–∏–±–∫–∞: {–æ—à–∏–±–∫–∞}")
            input("\n–ù–∞–∂–º–∏—Ç–µ Enter...")
            return
        
        –µ—Å–ª–∏ –¥–µ–π—Å—Ç–≤–∏–µ == 1:  # –í–∫–ª—é—á–µ–Ω–∏–µ
            # –ü–æ–∫–∞–∑—ã–≤–∞–µ–º —Ç–æ–ª—å–∫–æ –æ—Ç–∫–ª—é—á–µ–Ω–Ω—ã–µ —Ü–∏—Å—Ç–µ—Ä–Ω—ã
            –æ—Ç–∫–ª—é—á–µ–Ω–Ω—ã–µ = []
            –¥–ª—è —Ü–∏—Å—Ç–µ—Ä–Ω–∞ –≤ self.—Ü–∏—Å—Ç–µ—Ä–Ω—ã.values():
                –µ—Å–ª–∏ –Ω–µ —Ü–∏—Å—Ç–µ—Ä–Ω–∞.–≤–∫–ª—é—á–µ–Ω–∞ –∏ –Ω–µ —Ü–∏—Å—Ç–µ—Ä–Ω–∞.–∞–≤—Ç–æ–æ—Ç–∫–ª:
                    –æ—Ç–∫–ª—é—á–µ–Ω–Ω—ã–µ.append(—Ü–∏—Å—Ç–µ—Ä–Ω–∞)
            
            –µ—Å–ª–∏ –Ω–µ –æ—Ç–∫–ª—é—á–µ–Ω–Ω—ã–µ:
                print("\n–ù–µ—Ç —Ü–∏—Å—Ç–µ—Ä–Ω –¥–ª—è –≤–∫–ª—é—á–µ–Ω–∏—è.")
                input("\n–ù–∞–∂–º–∏—Ç–µ Enter...")
                return
            
            print("\n–¶–∏—Å—Ç–µ—Ä–Ω—ã, –¥–æ—Å—Ç—É–ø–Ω—ã–µ –¥–ª—è –≤–∫–ª—é—á–µ–Ω–∏—è:")
            –¥–ª—è i, —Ü–∏—Å—Ç–µ—Ä–Ω–∞ –≤ enumerate(–æ—Ç–∫–ª—é—á–µ–Ω–Ω—ã–µ, 1):
                print(f"{i}) {—Ü–∏—Å—Ç–µ—Ä–Ω–∞.–∏–¥}: {—Ü–∏—Å—Ç–µ—Ä–Ω–∞.—Ç–µ–∫—É—â–∏–π:.0f} / {—Ü–∏—Å—Ç–µ—Ä–Ω–∞.–º–∞–∫—Å:.0f} –ª")
            
            –≤—ã–±–æ—Ä, –æ—à–∏–±–∫–∞ = –ø—Ä–æ–≤–µ—Ä–∏—Ç—å_–≤–≤–æ–¥_—Ü–µ–ª–æ–≥–æ(input("\n–í—ã–±–µ—Ä–∏—Ç–µ —Ü–∏—Å—Ç–µ—Ä–Ω—É: "), 1, len(–æ—Ç–∫–ª—é—á–µ–Ω–Ω—ã–µ))
            –µ—Å–ª–∏ –æ—à–∏–±–∫–∞:
                print(f"–û—à–∏–±–∫–∞: {–æ—à–∏–±–∫–∞}")
                input("\n–ù–∞–∂–º–∏—Ç–µ Enter...")
                return
            
            —Ü–∏—Å—Ç–µ—Ä–Ω–∞ = –æ—Ç–∫–ª—é—á–µ–Ω–Ω—ã–µ[–≤—ã–±–æ—Ä - 1]
            —É—Å–ø–µ—Ö, —Å–æ–æ–±—â–µ–Ω–∏–µ = —Ü–∏—Å—Ç–µ—Ä–Ω–∞.–≤–∫–ª—é—á–∏—Ç—å()
            
            –µ—Å–ª–∏ —É—Å–ø–µ—Ö:
                print(f"\n‚úÖ {—Å–æ–æ–±—â–µ–Ω–∏–µ}")
                self.–¥–æ–±–∞–≤–∏—Ç—å_–≤_–∏—Å—Ç–æ—Ä–∏—é("–≤–∫–ª—é—á–µ–Ω–∏–µ", f"–í–∫–ª—é—á–µ–Ω–∏–µ —Ü–∏—Å—Ç–µ—Ä–Ω—ã {—Ü–∏—Å—Ç–µ—Ä–Ω–∞.–∏–¥}")
                self.—Å–æ—Ö—Ä–∞–Ω–∏—Ç—å_–≤—Å–µ_–¥–∞–Ω–Ω—ã–µ()
            –∏–Ω–∞—á–µ:
                print(f"\n‚ùå {—Å–æ–æ–±—â–µ–Ω–∏–µ}")
        
        –∏–Ω–∞—á–µ:  # –û—Ç–∫–ª—é—á–µ–Ω–∏–µ
            # –ü–æ–∫–∞–∑—ã–≤–∞–µ–º —Ç–æ–ª—å–∫–æ –≤–∫–ª—é—á–µ–Ω–Ω—ã–µ —Ü–∏—Å—Ç–µ—Ä–Ω—ã
            –≤–∫–ª—é—á–µ–Ω–Ω—ã–µ = []
            –¥–ª—è —Ü–∏—Å—Ç–µ—Ä–Ω–∞ –≤ self.—Ü–∏—Å—Ç–µ—Ä–Ω—ã.values():
                –µ—Å–ª–∏ —Ü–∏—Å—Ç–µ—Ä–Ω–∞.–≤–∫–ª—é—á–µ–Ω–∞:
                    –≤–∫–ª—é—á–µ–Ω–Ω—ã–µ.append(—Ü–∏—Å—Ç–µ—Ä–Ω–∞)
            
            –µ—Å–ª–∏ –Ω–µ –≤–∫–ª—é—á–µ–Ω–Ω—ã–µ:
                print("\n–ù–µ—Ç —Ü–∏—Å—Ç–µ—Ä–Ω –¥–ª—è –æ—Ç–∫–ª—é—á–µ–Ω–∏—è.")
                input("\n–ù–∞–∂–º–∏—Ç–µ Enter...")
                return
            
            print("\n–¶–∏—Å—Ç–µ—Ä–Ω—ã, –¥–æ—Å—Ç—É–ø–Ω—ã–µ –¥–ª—è –æ—Ç–∫–ª—é—á–µ–Ω–∏—è:")
            –¥–ª—è i, —Ü–∏—Å—Ç–µ—Ä–Ω–∞ –≤ enumerate(–≤–∫–ª—é—á–µ–Ω–Ω—ã–µ, 1):
                print(f"{i}) {—Ü–∏—Å—Ç–µ—Ä–Ω–∞.–∏–¥}: {—Ü–∏—Å—Ç–µ—Ä–Ω–∞.—Ç–µ–∫—É—â–∏–π:.0f} / {—Ü–∏—Å—Ç–µ—Ä–Ω–∞.–º–∞–∫—Å:.0f} –ª")
            
            –≤—ã–±–æ—Ä, –æ—à–∏–±–∫–∞ = –ø—Ä–æ–≤–µ—Ä–∏—Ç—å_–≤–≤–æ–¥_—Ü–µ–ª–æ–≥–æ(input("\n–í—ã–±–µ—Ä–∏—Ç–µ —Ü–∏—Å—Ç–µ—Ä–Ω—É: "), 1, len(–≤–∫–ª—é—á–µ–Ω–Ω—ã–µ))
            –µ—Å–ª–∏ –æ—à–∏–±–∫–∞:
                print(f"–û—à–∏–±–∫–∞: {–æ—à–∏–±–∫–∞}")
                input("\n–ù–∞–∂–º–∏—Ç–µ Enter...")
                return
            
            —Ü–∏—Å—Ç–µ—Ä–Ω–∞ = –≤–∫–ª—é—á–µ–Ω–Ω—ã—Ö[–≤—ã–±–æ—Ä - 1]
            —É—Å–ø–µ—Ö, —Å–æ–æ–±—â–µ–Ω–∏–µ = —Ü–∏—Å—Ç–µ—Ä–Ω–∞.–≤—ã–∫–ª—é—á–∏—Ç—å()
            
            –µ—Å–ª–∏ —É—Å–ø–µ—Ö:
                print(f"\n‚úÖ {—Å–æ–æ–±—â–µ–Ω–∏–µ}")
                self.–¥–æ–±–∞–≤–∏—Ç—å_–≤_–∏—Å—Ç–æ—Ä–∏—é("–æ—Ç–∫–ª—é—á–µ–Ω–∏–µ", f"–û—Ç–∫–ª—é—á–µ–Ω–∏–µ —Ü–∏—Å—Ç–µ—Ä–Ω—ã {—Ü–∏—Å—Ç–µ—Ä–Ω–∞.–∏–¥}")
                self.—Å–æ—Ö—Ä–∞–Ω–∏—Ç—å_–≤—Å–µ_–¥–∞–Ω–Ω—ã–µ()
            –∏–Ω–∞—á–µ:
                print(f"\n‚ùå {—Å–æ–æ–±—â–µ–Ω–∏–µ}")
        
        input("\n–ù–∞–∂–º–∏—Ç–µ Enter...")
    
    def –ø–æ–∫–∞–∑–∞—Ç—å_–∫–æ–ª–æ–Ω–∫–∏(self):
        """–ü–æ–∫–∞–∑—ã–≤–∞–µ—Ç —Å–æ—Å—Ç–æ—è–Ω–∏–µ –≤—Å–µ—Ö –∫–æ–ª–æ–Ω–æ–∫"""
        print("\n" + "="*50)
        print("–°–û–°–¢–û–Ø–ù–ò–ï –ö–û–õ–û–ù–û–ö")
        print("="*50)
        
        print("\n–í—Å–µ–≥–æ –∫–æ–ª–æ–Ω–æ–∫: 8")
        print("–ö–∞–∂–¥–∞—è –∫–æ–ª–æ–Ω–∫–∞ –ø–æ–¥–∫–ª—é—á–µ–Ω–∞ –∫ –æ–ø—Ä–µ–¥–µ–ª–µ–Ω–Ω—ã–º —Ü–∏—Å—Ç–µ—Ä–Ω–∞–º")
        print("-"*50)
        
        –¥–ª—è –∫–æ–ª–æ–Ω–∫–∞ in range(1, 9):
            print(f"\n–ö–û–õ–û–ù–ö–ê {–∫–æ–ª–æ–Ω–∫–∞}:")
            
            –µ—Å–ª–∏ –∫–æ–ª–æ–Ω–∫–∞ not in –°–•–ï–ú–ê_–ö–û–õ–û–ù–û–ö:
                print("  –ù–µ –ø–æ–¥–∫–ª—é—á–µ–Ω–∞")
                –ø—Ä–æ–¥–æ–ª–∂–∏—Ç—å
            
            –ø–æ–¥–∫–ª—é—á–µ–Ω–∏—è = –°–•–ï–ú–ê_–ö–û–õ–û–ù–û–ö[–∫–æ–ª–æ–Ω–∫–∞]
            
            –¥–ª—è —Ç–∏–ø_—Ç–æ–ø–ª–∏–≤–∞, id_—Ü–∏—Å—Ç–µ—Ä–Ω—ã in –ø–æ–¥–∫–ª—é—á–µ–Ω–∏—è.items():
                –µ—Å–ª–∏ id_—Ü–∏—Å—Ç–µ—Ä–Ω—ã in self.—Ü–∏—Å—Ç–µ—Ä–Ω—ã:
                    —Ü–∏—Å—Ç–µ—Ä–Ω–∞ = self.—Ü–∏—Å—Ç–µ—Ä–Ω—ã[id_—Ü–∏—Å—Ç–µ—Ä–Ω—ã]
                    
                    –µ—Å–ª–∏ —Ü–∏—Å—Ç–µ—Ä–Ω–∞.–≤–∫–ª—é—á–µ–Ω–∞:
                        —Å—Ç–∞—Ç—É—Å = "‚úì –†–ê–ë–û–¢–ê–ï–¢"
                    –∏–Ω–∞—á–µ:
                        –µ—Å–ª–∏ —Ü–∏—Å—Ç–µ—Ä–Ω–∞.–∞–≤—Ç–æ–æ—Ç–∫–ª:
                            —Å—Ç–∞—Ç—É—Å = "‚úó –ù–ï –†–ê–ë–û–¢–ê–ï–¢ (–Ω–∏–∑–∫–∏–π —É—Ä–æ–≤–µ–Ω—å)"
                        –∏–Ω–∞—á–µ:
                            —Å—Ç–∞—Ç—É—Å = "‚úó –ù–ï –†–ê–ë–û–¢–ê–ï–¢"
                    
                    print(f"  {—Ç–∏–ø_—Ç–æ–ø–ª–∏–≤–∞:6} ‚Üí {id_—Ü–∏—Å—Ç–µ—Ä–Ω—ã:12} [{—Å—Ç–∞—Ç—É—Å}]")
        
        input("\n–ù–∞–∂–º–∏—Ç–µ Enter...")
    
    def –∞–≤–∞—Ä–∏–π–Ω—ã–π_—Ä–µ–∂–∏–º(self):
        """–£–ø—Ä–∞–≤–ª–µ–Ω–∏–µ –∞–≤–∞—Ä–∏–π–Ω—ã–º —Ä–µ–∂–∏–º–æ–º"""
        print("\n" + "="*50)
        print("–ê–í–ê–†–ò–ô–ù–´–ô –†–ï–ñ–ò–ú")
        print("="*50)
        
        –µ—Å–ª–∏ self.–∞–≤–∞—Ä–∏—è["–∞–∫—Ç–∏–≤–Ω–∞"]:
            print(f"\nüö® –ê–í–ê–†–ò–ô–ù–´–ô –†–ï–ñ–ò–ú –ê–ö–¢–ò–í–ò–†–û–í–ê–ù!")
            print(f"–í—Ä–µ–º—è –∞–∫—Ç–∏–≤–∞—Ü–∏–∏: {self.–∞–≤–∞—Ä–∏—è['–≤—Ä–µ–º—è']}")
            print("\n–í—Å–µ —Ü–∏—Å—Ç–µ—Ä–Ω—ã –∑–∞–±–ª–æ–∫–∏—Ä–æ–≤–∞–Ω—ã.")
            print("–ó–∞–ø—Ä–∞–≤–∫–∞ –Ω–µ —Ä–∞–±–æ—Ç–∞–µ—Ç.")
            
            print("\n–í—ã–±–µ—Ä–∏—Ç–µ –¥–µ–π—Å—Ç–≤–∏–µ:")
            print("1) –û—Ç–∫–ª—é—á–∏—Ç—å –∞–≤–∞—Ä–∏–π–Ω—ã–π —Ä–µ–∂–∏–º")
            print("2) –í–µ—Ä–Ω—É—Ç—å—Å—è –≤ –º–µ–Ω—é")
            
            –≤—ã–±–æ—Ä, –æ—à–∏–±–∫–∞ = –ø—Ä–æ–≤–µ—Ä–∏—Ç—å_–≤–≤–æ–¥_—Ü–µ–ª–æ–≥–æ(input("\n> "), 1, 2)
            –µ—Å–ª–∏ –æ—à–∏–±–∫–∞:
                print(f"–û—à–∏–±–∫–∞: {–æ—à–∏–±–∫–∞}")
                input("\n–ù–∞–∂–º–∏—Ç–µ Enter...")
                return
            
            –µ—Å–ª–∏ –≤—ã–±–æ—Ä == 1:
                –ø–æ–¥—Ç–≤–µ—Ä–¥–∏—Ç—å = input("\n–í—ã —É–≤–µ—Ä–µ–Ω—ã? (–¥/–Ω): ").lower()
                –µ—Å–ª–∏ –ø–æ–¥—Ç–≤–µ—Ä–¥–∏—Ç—å == '–¥':
                    self.–∞–≤–∞—Ä–∏—è["–∞–∫—Ç–∏–≤–Ω–∞"] = False
                    self.–∞–≤–∞—Ä–∏—è["–≤—Ä–µ–º—è"] = None
                    
                    self.–¥–æ–±–∞–≤–∏—Ç—å_–≤_–∏—Å—Ç–æ—Ä–∏—é(
                        "–∞–≤–∞—Ä–∏—è_–≤—ã–∫–ª",
                        "–û—Ç–∫–ª—é—á–µ–Ω–∏–µ –∞–≤–∞—Ä–∏–π–Ω–æ–≥–æ —Ä–µ–∂–∏–º–∞"
                    )
                    
                    self.—Å–æ—Ö—Ä–∞–Ω–∏—Ç—å_–≤—Å–µ_–¥–∞–Ω–Ω—ã–µ()
                    
                    print("\n‚úÖ –ê–≤–∞—Ä–∏–π–Ω—ã–π —Ä–µ–∂–∏–º –æ—Ç–∫–ª—é—á–µ–Ω.")
                    print("\n–í–ê–ñ–ù–û: –¶–∏—Å—Ç–µ—Ä–Ω—ã –æ—Å—Ç–∞–ª–∏—Å—å –≤ —Ç–µ–∫—É—â–µ–º —Å–æ—Å—Ç–æ—è–Ω–∏–∏.")
                    print("–î–ª—è –≤–∫–ª—é—á–µ–Ω–∏—è –∏—Å–ø–æ–ª—å–∑—É–π—Ç–µ –º–µ–Ω—é —É–ø—Ä–∞–≤–ª–µ–Ω–∏—è —Ü–∏—Å—Ç–µ—Ä–Ω–∞–º–∏.")
                –∏–Ω–∞—á–µ:
                    print("\n–û—Ç–º–µ–Ω–∞.")
            
            input("\n–ù–∞–∂–º–∏—Ç–µ Enter...")
            return
        
        # –ï—Å–ª–∏ –∞–≤–∞—Ä–∏–π–Ω—ã–π —Ä–µ–∂–∏–º –Ω–µ –∞–∫—Ç–∏–≤–µ–Ω
        print("\nüö® –í–ù–ò–ú–ê–ù–ò–ï! –í–´ –°–û–ë–ò–†–ê–ï–¢–ï–°–¨ –ê–ö–¢–ò–í–ò–†–û–í–ê–¢–¨ –ê–í–ê–†–ò–ô–ù–´–ô –†–ï–ñ–ò–ú!")
        print("\n–ü–æ—Å–ª–µ–¥—Å—Ç–≤–∏—è:")
        print("1. –í—Å–µ —Ü–∏—Å—Ç–µ—Ä–Ω—ã –±—É–¥—É—Ç –∑–∞–±–ª–æ–∫–∏—Ä–æ–≤–∞–Ω—ã")
        print("2. –ó–∞–ø—Ä–∞–≤–∫–∞ –ø—Ä–µ–∫—Ä–∞—Ç–∏—Ç —Ä–∞–±–æ—Ç—É")
        print("3. –ë—É–¥–µ—Ç –∑–∞—Ñ–∏–∫—Å–∏—Ä–æ–≤–∞–Ω–æ –∞–≤–∞—Ä–∏–π–Ω–æ–µ —Å–æ–±—ã—Ç–∏–µ")
        print("4. –ò–º–∏—Ç–∏—Ä—É–µ—Ç—Å—è –≤—ã–∑–æ–≤ –∞–≤–∞—Ä–∏–π–Ω—ã—Ö —Å–ª—É–∂–±")
        
        –ø–æ–¥—Ç–≤–µ—Ä–¥–∏—Ç—å = input("\n–ü–æ–¥—Ç–≤–µ—Ä–¥–∏—Ç—å –∞–∫—Ç–∏–≤–∞—Ü–∏—é? (–¥/–Ω): ").lower()
        –µ—Å–ª–∏ –ø–æ–¥—Ç–≤–µ—Ä–¥–∏—Ç—å != '–¥':
            print("\n–û—Ç–º–µ–Ω–∞.")
            input("\n–ù–∞–∂–º–∏—Ç–µ Enter...")
            return
        
        # –ê–∫—Ç–∏–≤–∞—Ü–∏—è –∞–≤–∞—Ä–∏–π–Ω–æ–≥–æ —Ä–µ–∂–∏–º–∞
        # 1. –ë–ª–æ–∫–∏—Ä—É–µ–º –≤—Å–µ —Ü–∏—Å—Ç–µ—Ä–Ω—ã
        –¥–ª—è —Ü–∏—Å—Ç–µ—Ä–Ω–∞ in self.—Ü–∏—Å—Ç–µ—Ä–Ω—ã.values():
            —Ü–∏—Å—Ç–µ—Ä–Ω–∞.–≤–∫–ª—é—á–µ–Ω–∞ = False
            —Ü–∏—Å—Ç–µ—Ä–Ω–∞.–∞–≤—Ç–æ–æ—Ç–∫–ª = False
        
        # 2. –£—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º –∞–≤–∞—Ä–∏–π–Ω—ã–π —Ä–µ–∂–∏–º
        self.–∞–≤–∞—Ä–∏—è["–∞–∫—Ç–∏–≤–Ω–∞"] = True
        self.–∞–≤–∞—Ä–∏—è["–≤—Ä–µ–º—è"] = –ø–æ–ª—É—á–∏—Ç—å_–¥–∞—Ç—É_–≤—Ä–µ–º—è()
        
        # 3. –ó–∞–ø–∏—Å—å –≤ –∏—Å—Ç–æ—Ä–∏—é
        self.–¥–æ–±–∞–≤–∏—Ç—å_–≤_–∏—Å—Ç–æ—Ä–∏—é(
            "–∞–≤–∞—Ä–∏—è_–≤–∫–ª",
            "–ê–∫—Ç–∏–≤–∞—Ü–∏—è –∞–≤–∞—Ä–∏–π–Ω–æ–≥–æ —Ä–µ–∂–∏–º–∞",
            {"–≤—Ä–µ–º—è": self.–∞–≤–∞—Ä–∏—è["–≤—Ä–µ–º—è"]}
        )
        
        # 4. –°–æ—Ö—Ä–∞–Ω–µ–Ω–∏–µ
        self.—Å–æ—Ö—Ä–∞–Ω–∏—Ç—å_–≤—Å–µ_–¥–∞–Ω–Ω—ã–µ()
        
        # 5. –ò–º–∏—Ç–∞—Ü–∏—è –≤—ã–∑–æ–≤–∞ —Å–ª—É–∂–±
        print("\n" + "!"*60)
        print("üö® –ê–í–ê–†–ò–ô–ù–´–ô –†–ï–ñ–ò–ú –ê–ö–¢–ò–í–ò–†–û–í–ê–ù! üö®")
        print("!"*60)
        print("\n–í—Å–µ —Ü–∏—Å—Ç–µ—Ä–Ω—ã –∑–∞–±–ª–æ–∫–∏—Ä–æ–≤–∞–Ω—ã.")
        print("–ó–∞–ø—Ä–∞–≤–∫–∞ –Ω–µ —Ä–∞–±–æ—Ç–∞–µ—Ç.")
        print("\n–ò–º–∏—Ç–∞—Ü–∏—è –≤—ã–∑–æ–≤–∞ –∞–≤–∞—Ä–∏–π–Ω—ã—Ö —Å–ª—É–∂–±...")
        print("  112 - –°–ª—É–∂–±–∞ —Å–ø–∞—Å–µ–Ω–∏—è")
        print("  104 - –ì–∞–∑–æ–≤–∞—è —Å–ª—É–∂–±–∞")
        print("  –î–µ–∂—É—Ä–Ω—ã–π –¥–∏—Å–ø–µ—Ç—á–µ—Ä: 8-800-555-35-35")
        print("\n–î–∞–Ω–Ω—ã–µ –∑–∞—Ñ–∏–∫—Å–∏—Ä–æ–≤–∞–Ω—ã.")
        
        input("\n–ù–∞–∂–º–∏—Ç–µ Enter...")
    
    def –≥–ª–∞–≤–Ω–æ–µ_–º–µ–Ω—é(self):
        """–ì–ª–∞–≤–Ω–æ–µ –º–µ–Ω—é –ø—Ä–æ–≥—Ä–∞–º–º—ã"""
        –ø–æ–∫–∞ True:
            # –û—á–∏—Å—Ç–∫–∞ —ç–∫—Ä–∞–Ω–∞
            –µ—Å–ª–∏ os.name == 'nt':  # Windows
                os.system('cls')
            –∏–Ω–∞—á–µ:  # Linux/Mac
                os.system('clear')
            
            # –ó–∞–≥–æ–ª–æ–≤–æ–∫
            print("="*60)
            print("–ê–ó–° <<–°–µ–≤–µ—Ä–ù–µ—Ñ—Ç—å>>")
            print("–°–∏—Å—Ç–µ–º–∞ —É–ø—Ä–∞–≤–ª–µ–Ω–∏—è –∑–∞–ø—Ä–∞–≤–æ—á–Ω–æ–π —Å—Ç–∞–Ω—Ü–∏–µ–π")
            print("="*60)
            
            # –ü—Ä–µ–¥—É–ø—Ä–µ–∂–¥–µ–Ω–∏—è
            –µ—Å–ª–∏ self.–∞–≤–∞—Ä–∏—è["–∞–∫—Ç–∏–≤–Ω–∞"]:
                print("\n" + "!"*60)
                print("–í–ù–ò–ú–ê–ù–ò–ï: –ê–ö–¢–ò–í–ò–†–û–í–ê–ù –ê–í–ê–†–ò–ô–ù–´–ô –†–ï–ñ–ò–ú!")
                print("–ó–ê–ü–†–ê–í–ö–ê –ù–ï –†–ê–ë–û–¢–ê–ï–¢")
                print("!"*60)
            –∏–Ω–∞—á–µ:
                –æ—Ç–∫–ª—é—á–µ–Ω–Ω—ã–µ = self.–ø—Ä–æ–≤–µ—Ä–∏—Ç—å_–æ—Ç–∫–ª—é—á–µ–Ω–Ω—ã–µ_—Ü–∏—Å—Ç–µ—Ä–Ω—ã()
                –µ—Å–ª–∏ –æ—Ç–∫–ª—é—á–µ–Ω–Ω—ã–µ:
                    print("\n–í–ù–ò–ú–ê–ù–ò–ï!")
                    print("–û–±–Ω–∞—Ä—É–∂–µ–Ω—ã –æ—Ç–∫–ª—é—á—ë–Ω–Ω—ã–µ —Ü–∏—Å—Ç–µ—Ä–Ω—ã:")
                    –¥–ª—è —Ü–∏—Å—Ç–µ—Ä–Ω–∞ in –æ—Ç–∫–ª—é—á–µ–Ω–Ω—ã–µ:
                        print(f" - {—Ü–∏—Å—Ç–µ—Ä–Ω–∞}")
            
            print("\n" + "-"*60)
            print("–ì–õ–ê–í–ù–û–ï –ú–ï–ù–Æ")
            print("-"*60)
            
            # –ü—É–Ω–∫—Ç—ã –º–µ–Ω—é
            print("\n–í—ã–±–µ—Ä–∏—Ç–µ –¥–µ–π—Å—Ç–≤–∏–µ:")
            print(" 1) –û–±—Å–ª—É–∂–∏—Ç—å –∫–ª–∏–µ–Ω—Ç–∞ (–∫–∞—Å—Å–∞)")
            print(" 2) –ü—Ä–æ–≤–µ—Ä–∏—Ç—å —Å–æ—Å—Ç–æ—è–Ω–∏–µ —Ü–∏—Å—Ç–µ—Ä–Ω")
            print(" 3) –û—Ñ–æ—Ä–º–∏—Ç—å –ø–æ–ø–æ–ª–Ω–µ–Ω–∏–µ —Ç–æ–ø–ª–∏–≤–∞")
            print(" 4) –ë–∞–ª–∞–Ω—Å –∏ —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞")
            print(" 5) –ò—Å—Ç–æ—Ä–∏—è –æ–ø–µ—Ä–∞—Ü–∏–π")
            print(" 6) –ü–µ—Ä–µ–∫–∞—á–∫–∞ —Ç–æ–ø–ª–∏–≤–∞ –º–µ–∂–¥—É —Ü–∏—Å—Ç–µ—Ä–Ω–∞–º–∏")
            print(" 7) –í–∫–ª—é—á–µ–Ω–∏–µ / –æ—Ç–∫–ª—é—á–µ–Ω–∏–µ —Ü–∏—Å—Ç–µ—Ä–Ω")
            print(" 8) –°–æ—Å—Ç–æ—è–Ω–∏–µ –∫–æ–ª–æ–Ω–æ–∫")
            print(" 9) EMERGENCY - –∞–≤–∞—Ä–∏–π–Ω–∞—è —Å–∏—Ç—É–∞—Ü–∏—è")
            print(" 0) –í—ã—Ö–æ–¥")
            
            try:
                –≤—ã–±–æ—Ä = input("\n> ")
                
                –µ—Å–ª–∏ –≤—ã–±–æ—Ä == "0":
                    print("\n–°–æ—Ö—Ä–∞–Ω–µ–Ω–∏–µ –¥–∞–Ω–Ω—ã—Ö...")
                    self.—Å–æ—Ö—Ä–∞–Ω–∏—Ç—å_–≤—Å–µ_–¥–∞–Ω–Ω—ã–µ()
                    print("–í—ã—Ö–æ–¥ –∏–∑ –ø—Ä–æ–≥—Ä–∞–º–º—ã.")
                    –ø–µ—Ä–µ—Ä—ã–≤
                
                elif –≤—ã–±–æ—Ä == "1":
                    self.–æ–±—Å–ª—É–∂–∏—Ç—å_–∫–ª–∏–µ–Ω—Ç–∞()
                elif –≤—ã–±–æ—Ä == "2":
                    self.–ø–æ–∫–∞–∑–∞—Ç—å_—Ü–∏—Å—Ç–µ—Ä–Ω—ã()
                elif –≤—ã–±–æ—Ä == "3":
                    self.–ø–æ–ø–æ–ª–Ω–∏—Ç—å_—Ü–∏—Å—Ç–µ—Ä–Ω—É()
                elif –≤—ã–±–æ—Ä == "4":
                    self.–ø–æ–∫–∞–∑–∞—Ç—å_—Å—Ç–∞—Ç–∏—Å—Ç–∏–∫—É()
                elif –≤—ã–±–æ—Ä == "5":
                    self.–ø–æ–∫–∞–∑–∞—Ç—å_–∏—Å—Ç–æ—Ä–∏—é()
                elif –≤—ã–±–æ—Ä == "6":
                    self.–ø–µ—Ä–µ–∫–∞—á–∞—Ç—å_—Ç–æ–ø–ª–∏–≤–æ()
                elif –≤—ã–±–æ—Ä == "7":
                    self.—É–ø—Ä–∞–≤–ª–µ–Ω–∏–µ_—Ü–∏—Å—Ç–µ—Ä–Ω–∞–º–∏()
                elif –≤—ã–±–æ—Ä == "8":
                    self.–ø–æ–∫–∞–∑–∞—Ç—å_–∫–æ–ª–æ–Ω–∫–∏()
                elif –≤—ã–±–æ—Ä == "9":
                    self.–∞–≤–∞—Ä–∏–π–Ω—ã–π_—Ä–µ–∂–∏–º()
                –∏–Ω–∞—á–µ:
                    print("\n–ù–µ–≤–µ—Ä–Ω—ã–π –≤—ã–±–æ—Ä!")
                    input("–ù–∞–∂–º–∏—Ç–µ Enter –¥–ª—è –ø—Ä–æ–¥–æ–ª–∂–µ–Ω–∏—è...")
            
            except KeyboardInterrupt:
                print("\n\n–í—ã—Ö–æ–¥ –∏–∑ –ø—Ä–æ–≥—Ä–∞–º–º—ã.")
                self.—Å–æ—Ö—Ä–∞–Ω–∏—Ç—å_–≤—Å–µ_–¥–∞–Ω–Ω—ã–µ()
                –ø–µ—Ä–µ—Ä—ã–≤
            except Exception as e:
                print(f"\n–û—à–∏–±–∫–∞: {e}")
                input("–ù–∞–∂–º–∏—Ç–µ Enter –¥–ª—è –ø—Ä–æ–¥–æ–ª–∂–µ–Ω–∏—è...")

