        else:
            return False, f"–ù–µ–ª—å–∑—è –≤–∫–ª—é—á–∏—Ç—å: —É—Ä–æ–≤–µ–Ω—å –Ω–∏–∂–µ –º–∏–Ω–∏–º—É–º–∞ ({self.–º–∏–Ω:.0f} –ª)"
    
    def –≤—ã–∫–ª—é—á–∏—Ç—å(self):
        """–í—ã–∫–ª—é—á–∞–µ—Ç —Ü–∏—Å—Ç–µ—Ä–Ω—É –≤—Ä—É—á–Ω—É—é"""
        self.–≤–∫–ª—é—á–µ–Ω–∞ = False
        self.–∞–≤—Ç–æ–æ—Ç–∫–ª = False
        return True, "–¶–∏—Å—Ç–µ—Ä–Ω–∞ –≤—ã–∫–ª—é—á–µ–Ω–∞"
    
    def –ø–æ–ª—É—á–∏—Ç—å_—Å—Ç–∞—Ç—É—Å(self):
        """–í–æ–∑–≤—Ä–∞—â–∞–µ—Ç —Å—Ç—Ä–æ–∫—É —Å—Ç–∞—Ç—É—Å–∞"""
        if self.–≤–∫–ª—é—á–µ–Ω–∞:
            return "–í–ö–õ"
        elif self.–∞–≤—Ç–æ–æ—Ç–∫–ª:
            return "–í–´–ö–õ (–∞–≤—Ç–æ)"
        else:
            return "–í–´–ö–õ"
    
    def –ø–æ–ª—É—á–∏—Ç—å_–ø—Ä–æ—Ü–µ–Ω—Ç(self):
        """–í–æ–∑–≤—Ä–∞—â–∞–µ—Ç –∑–∞–ø–æ–ª–Ω–µ–Ω–Ω–æ—Å—Ç—å –≤ –ø—Ä–æ—Ü–µ–Ω—Ç–∞—Ö"""
